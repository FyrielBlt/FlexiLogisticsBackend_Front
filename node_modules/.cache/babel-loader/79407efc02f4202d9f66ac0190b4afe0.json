{"ast":null,"code":"import { defineComponent } from \"vue\"; //import { useSidebar } from \"../hooks/useSidebar\";\n\nimport router from \"../router\";\nimport { ref } from \"vue\"; //import { Axios } from \"axios\";\n\nexport default defineComponent({\n  data() {\n    return {\n      dropdownOpen: ref(false),\n      isOpen: true,\n      notificationOpen: ref(false),\n      offres: [],\n      name: localStorage.getItem('name'),\n      prenom: localStorage.getItem('prenom'),\n      image: localStorage.getItem(\"imagesrc\"),\n      am_pm: '',\n      date: new Date().toJSON().slice(0, 10).replace(/-/g, '/')\n    };\n  },\n\n  created() {\n    setInterval(this.showTime, 1000);\n  },\n\n  methods: {\n    isopen() {\n      this.$emit('openSide', true);\n    },\n\n    showTime() {\n      let time = new Date();\n      let hour = time.getHours();\n      let min = time.getMinutes();\n      let sec = time.getSeconds();\n      let hr = 0;\n      this.am_pm = \"AM\";\n\n      if (hour > 12) {\n        hour -= 12;\n        this.am_pm = \"PM\";\n      }\n\n      if (hour == 0) {\n        hr = 12;\n        this.am_pm = \"AM\";\n      }\n\n      hour = hour < 10 ? \"0\" + hour : hour;\n      min = min < 10 ? \"0\" + min : min;\n      sec = sec < 10 ? \"0\" + sec : sec;\n      let currentTime = hour + \":\" + min + \":\" + sec + this.am_pm;\n      document.getElementById(\"clock\").innerHTML = currentTime;\n    },\n\n    logout() {\n      localStorage.removeItem(\"token\");\n      localStorage.removeItem(\"type\");\n      localStorage.removeItem(\"email\");\n      localStorage.removeItem(\"name\");\n      localStorage.removeItem(\"nom\");\n      localStorage.removeItem(\"prenom\");\n      localStorage.removeItem(\"societe\");\n      localStorage.removeItem(\"image\");\n      localStorage.removeItem(\"iduser\");\n      localStorage.removeItem(\"jwt\");\n      localStorage.removeItem(\"jwtt\");\n      localStorage.removeItem(\"jwtc\");\n      localStorage.removeItem(\"clientid\");\n      localStorage.removeItem(\"id\");\n      localStorage.removeItem(\"idtransporteur\");\n      localStorage.removeItem(\"idRole\");\n      localStorage.removeItem(\"imagesrc\");\n      router.push(\"/\");\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAqFA,SAASA,eAAT,QAAgC,KAAhC,C,CACA;;AACA,OAAOC,MAAP,MAAmB,WAAnB;AACA,SAASC,GAAT,QAAoB,KAApB,C,CACA;;AACA,eAAeF,eAAe,CAAC;AAC7BG,MAAI,GAAG;AACL,WAAO;AACLC,kBAAY,EAAEF,GAAG,CAAC,KAAD,CADZ;AAELG,YAAM,EAAE,IAFH;AAGLC,sBAAgB,EAAEJ,GAAG,CAAC,KAAD,CAHhB;AAILK,YAAM,EAAE,EAJH;AAKLC,UAAI,EAAEC,YAAY,CAACC,OAAb,CAAqB,MAArB,CALD;AAMLC,YAAM,EAAEF,YAAY,CAACC,OAAb,CAAqB,QAArB,CANH;AAOLE,WAAK,EAAEH,YAAY,CAACC,OAAb,CAAqB,UAArB,CAPF;AAQLG,WAAK,EAAE,EARF;AASLC,UAAI,EAAE,IAAIC,IAAJ,GAAWC,MAAX,GAAoBC,KAApB,CAA0B,CAA1B,EAA6B,EAA7B,EAAiCC,OAAjC,CAAyC,IAAzC,EAA+C,GAA/C;AATD,KAAP;AAWD,GAb4B;;AAc7BC,SAAO,GAAG;AACRC,eAAW,CAAC,KAAKC,QAAN,EAAgB,IAAhB,CAAX;AAED,GAjB4B;;AAkB7BC,SAAO,EAAE;AACPC,UAAM,GAAE;AACL,WAAKC,KAAL,CAAW,UAAX,EAAsB,IAAtB;AACF,KAHM;;AAIPH,YAAQ,GAAG;AACT,UAAII,IAAG,GAAI,IAAIV,IAAJ,EAAX;AACA,UAAIW,IAAG,GAAID,IAAI,CAACE,QAAL,EAAX;AACA,UAAIC,GAAE,GAAIH,IAAI,CAACI,UAAL,EAAV;AACA,UAAIC,GAAE,GAAIL,IAAI,CAACM,UAAL,EAAV;AACA,UAAIC,EAAC,GAAI,CAAT;AACA,WAAKnB,KAAL,GAAa,IAAb;;AAEA,UAAIa,IAAG,GAAI,EAAX,EAAe;AACbA,YAAG,IAAK,EAAR;AACA,aAAKb,KAAL,GAAa,IAAb;AACF;;AACA,UAAIa,IAAG,IAAK,CAAZ,EAAe;AACbM,UAAC,GAAI,EAAL;AACA,aAAKnB,KAAL,GAAa,IAAb;AACF;;AAEAa,UAAG,GAAIA,IAAG,GAAI,EAAP,GAAY,MAAMA,IAAlB,GAAyBA,IAAhC;AACAE,SAAE,GAAIA,GAAE,GAAI,EAAN,GAAW,MAAMA,GAAjB,GAAuBA,GAA7B;AACAE,SAAE,GAAIA,GAAE,GAAI,EAAN,GAAW,MAAMA,GAAjB,GAAuBA,GAA7B;AAEA,UAAIG,WAAU,GAAIP,IAAG,GAAI,GAAP,GACdE,GADc,GACR,GADQ,GACFE,GADE,GACI,KAAKjB,KAD3B;AAGAqB,cAAQ,CAACC,cAAT,CAAwB,OAAxB,EACGC,SADH,GACeH,WADf;AAED,KA9BM;;AA+BPI,UAAM,GAAG;AAEP5B,kBAAY,CAAC6B,UAAb,CAAwB,OAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CAAwB,MAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CAAwB,OAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CAAwB,MAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CAAwB,KAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CAAwB,QAAxB;AACA7B,kBAAY,CAAC6B,UAAb,CA