{"ast":null,"code":"import pdf from \"pdfvuer\";\nimport \"pdfjs-dist/build/pdf.worker.entry\";\nimport BreadCrumb from \"../../../components/Intermediaire/BreadCrumb.vue\";\nimport axios from \"axios\";\nimport Url from \"../../../store/Api\"; //import { mapGetters } from \"vuex\";\n\nexport default {\n  components: {\n    BreadCrumb,\n    pdf\n  },\n\n  data() {\n    return {\n      file: \"\",\n      imageFile: \"\",\n      numPage: 1\n    };\n  },\n\n  async created() {\n    await axios.get(Url + \"FactureTransporteurs/\" + this.$route.params.facture, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      this.file = res.data;\n    });\n  },\n\n  methods: {\n    Next() {\n      if (this.numPage < this.pageCount) {\n        this.numPage++;\n      }\n    },\n\n    Previous() {\n      if (this.numPage > 1) {\n        this.numPage--;\n      }\n    },\n\n    async upload() {\n      let facture = new FormData();\n      facture.append(\"IdFactTransporteur\", this.file.idFactTransporteur);\n      facture.append(\"EtatFacture\", \"En cour de payement\");\n      facture.append(\"IdOffre\", this.file.idOffre);\n      facture.append(\"FactureFile\", this.file.factureFile);\n      facture.append(\"PayementFile\", null);\n      facture.append(\"ImageFile\", this.imageFile);\n      await axios.put(Url + \"FactureTransporteurs/\" + this.file.idFactTransporteur, facture, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n        }\n      }).then(res => {\n        this.file = res.data;\n        console.log(res.data);\n      });\n    },\n\n    // ajouter image\n    FileSelected(event) {\n      this.imageFile = event.target.files[0];\n      console.log(this.imageFile);\n    }\n\n  },\n  watch: {}\n};","map":{"version":3,"mappings":"AAyIA,OAAOA,GAAP,MAAgB,SAAhB;AACA,OAAO,mCAAP;AACA,OAAOC,UAAP,MAAuB,kDAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,oBAAhB,C,CAEA