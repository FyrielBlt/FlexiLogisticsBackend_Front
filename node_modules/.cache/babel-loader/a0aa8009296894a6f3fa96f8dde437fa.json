A/C,EAAsD;AAC3D,aAAKO,MAAL,GAAc,IAAd;AACA,aAAKhC,MAAL,GAAc,KAAd;AACF;AAEA;;AA/FK;AAnDI,CAAf","names":["axios","datetime","props","components","data","idchauffeur","codevehicule","localStorage","getItem","Destinaton","depart","statut","open","cinchauffeur","chauffeur","sucess","error","dateheure","nextpage","chauffeurscamions","idtransporteur","i","camion","villes","iddepart","iddestination","created","get","then","resp","idTransporteur","response","catch","console","log","methods","close","location","replace","ajouterTrajet","add","idVille","post","idVille1","idVille2","idCamion","date","error2","description","desc","idEtat","prix","prixFinale","idDemande","demandeid2"],"sourceRoot":"","sources":["C:\\Users\\user\\Desktop\\frontfinal\\src\\views\\Transporteur\\Trajets\\ModalTrajetAdd .vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Breadcrumb -->\r\n    <!--  <Breadcrumb breadcrumb=\"Modal\" /> -->\r\n    <button @click=\"open = true\">\r\n      <svg\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\n        width=\"30\"\r\n        height=\"25\"\r\n        fill=\"currentColor\"\r\n        class=\"bi bi-plus-circle-fill\"\r\n        viewBox=\"0 0 16 16\"\r\n      >\r\n        <path\r\n          d=\"M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z\"\r\n        />\r\n      </svg>\r\n    </button>\r\n\r\n    <div\r\n      :class=\"`modal ${\r\n        !open && 'opacity-0 pointer-events-none'\r\n      } z-50 fixed w-full h-full top-0 left-0 flex items-center justify-center`\"\r\n    >\r\n      <div\r\n        @click=\"open = false\"\r\n        class=\"absolute w-full h-full bg-gray-900 opacity-50 modal-overlay\"\r\n      ></div>\r\n\r\n      <div\r\n        class=\"z-50 w-11/12 mx-auto overflow-y-auto bg-white rounded shadow-lg modal-container md:max-w-md\"\r\n      >\r\n        <div\r\n          class=\"absolute top-0 right-0 z-50 flex flex-col items-center mt-4 mr-4 text-sm text-white cursor-pointer modal-close\"\r\n        >\r\n          <svg\r\n            class=\"text-white fill-current\"\r\n            xmlns=\"http://www.w3.org/2000/svg\"\r\n            width=\"18\"\r\n            height=\"18\"\r\n            viewBox=\"0 0 18 18\"\r\n          >\r\n            <path\r\n              d=\"M14.53 4.53l-1.06-1.06L9 7.94 4.53 3.47 3.47 4.53 7.94 9l-4.47 4.47 1.06 1.06L9 10.06l4.47 4.47 1.06-1.06L10.06 9z\"\r\n            />\r\n          </svg>\r\n          <span class=\"text-sm\">(Esc)</span>\r\n        </div>\r\n\r\n        <!-- Add margin if you want to see some of the overlay behind the modal-->\r\n        <div class=\"px-6 py-4 text-left modal-content\">\r\n          <!--Title-->\r\n          <div class=\"flex items-center justify-between pb-3\">\r\n            <p class=\"text-2xl font-bold\">Ajouter Trajet</p>\r\n          </div>\r\n\r\n          <!--Body-->\r\n          <div class=\"mt-8\">\r\n            <div class=\"mt-4\">\r\n              <div class=\"p-6 bg-white rounded-md shadow-md\">\r\n                <div class=\"grid grid-cols-1 gap-6 mt-4 sm:grid-cols-2\">\r\n                  <div v-if=\"this.add!=true\">\r\n                    <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    > camion\r\n                    </label>\r\n                    <select\r\n                      v-model=\"camion\"\r\n                      class=\"w-full mt-2 border-gray-200 rounded-md focus:border-indigo-600 focus:ring focus:ring-opacity-40 focus:ring-indigo-500\"\r\n                    >\r\n                      <option\r\n                        v-for=\"column in this.chauffeurscamions\"\r\n                        :key=\"column.codevehicule\"\r\n                        :value=\"column.idcamion \"\r\n                      >\r\n                      {{ column.codevehicule}}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                  <div v-if=\"this.add==true\">\r\n                    <label class=\"text-gray-700\" for=\"username\"\r\n                      >Code Véhicule</label\r\n                    >\r\n                    <input\r\n                      disabled\r\n                      class=\"w-full mt-2 border-gray-200 rounded-md focus:border-indigo-600 focus:ring focus:ring-opacity-40 focus:ring-indigo-500\"\r\n                      type=\"text\"\r\n                      v-model=\"this.codevehicule\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >ville départ\r\n                  </label>\r\n                  <select\r\n                    v-model=\"depart\"\r\n                    class=\"w-full mt-2 border-gray-200 rounded-md focus:border-indigo-600 focus:ring focus:ring-opacity-40 focus:ring-indigo-500\"\r\n                  >\r\n                    <option v-for=\"column in this.villes\" :key=\"column\">\r\n                      {{ column.nomVille }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >Destination\r\n                  </label>\r\n                  <select\r\n                    v-model=\"Destinaton\"\r\n                    class=\"w-full mt-2 border-gray-200 rounded-md focus:border-indigo-600 focus:ring focus:ring-opacity-40 focus:ring-indigo-500\"\r\n                  >\r\n                    <option v-for=\"column in this.villes\" :key=\"column\">\r\n                      {{ column.nomVille }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >Date/heure\r\n                  </label>\r\n                  <br>\r\n                  <input\r\n                    type=\"datetime-local\"\r\n                    width=\"300px\"\r\n                    v-model=\"dateheure\"\r\n                  />\r\n                </div>\r\n                <div class=\"flex justify-end mt-4\">\r\n                  <button\r\n                    @click=\"close\"\r\n                    class=\"p-3 px-6 py-3 mr-2 text-indigo-500 bg-transparent rounded-lg hover:bg-gray-100 hover:text-indigo-400 focus:outline-none\"\r\n                  >\r\n                    Close\r\n                  </button>\r\n                  <button\r\n                    @click=\"ajouterTrajet\"\r\n                    class=\"px-4 py-2 text-gray-200 bg-gray-800 rounded-md hover:bg-gray-700 focus:outline-none focus:bg-gray-700\"\r\n                  >\r\n                    Save\r\n                  </button>\r\n                </div>\r\n\r\n                <div v-if=\"this.error == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-red-500\"\r\n                      >Merci de remplir tout les champs</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">error!</p>\r\n                  </div>\r\n                </div>\r\n                <div v-if=\"this.error2 == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-red-500\"\r\n                      >Depart et Destination ne peuvent pas etre\r\n                      identiques</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">error!</p>\r\n                  </div>\r\n                </div>\r\n                <div v-if=\"this.sucess == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-green-500\"\r\n                      >Nouvelle véhicule de transport</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">Ajout réussi!</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport datetime from \"vuejs-datetimepicker\";\r\n\r\nexport default {\r\n  props:[\"add\" , 'desc',\"prix\",\"demandeid2\"],\r\n  components: {\r\n    datetime,\r\n  },\r\n  data() {\r\n    return {\r\n      idchauffeur: \"\",\r\n      codevehicule: localStorage.getItem('moncamion'),\r\n      Destinaton: \"\",\r\n      depart: \"\",\r\n      statut: \"\",\r\n      open: false,\r\n      cinchauffeur: \"\",\r\n      chauffeur: \"\",\r\n      sucess: false,\r\n      error: false,\r\n      dateheure:'',\r\n      nextpage: 1,\r\n      chauffeurscamions: [],\r\n      idtransporteur: \"\",\r\n      i: 0,\r\n      camion:'',\r\n      villes: [],\r\n      iddepart: \"\",\r\n      iddestination: \"\",\r\n      chauffeurscamions:[],\r\n    };\r\n  },\r\n  created() {\r\n       axios\r\n      .get(\r\n        \"http://localhost:5000/api/Transporteurs/\" +\r\n          localStorage.getItem(\"iduser\") +\r\n          \"/iduser\"\r\n      ).then((resp)=>{\r\n      axios\r\n      .get(\"http://localhost:5000/api/Camions/\"+resp.data.idTransporteur)\r\n      .then((response) =>{\r\n      this.chauffeurscamions= response.data\r\n      }\r\n      )\r\n      .catch((error) => console.log(error)) \r\n      })\r\n    axios\r\n      .get(\"http://localhost:5000/api/Villes\")\r\n      .then((response) => {\r\n        this.villes = response.data;\r\n      })\r\n      .catch((error) => console.log(error));\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.error = false;\r\n      this.sucess = false;\r\n      this.open = false;\r\n      location.replace(\"camions\");\r\n    },\r\n    ajouterTrajet() {\r\n      switch(this.add) {\r\n  case true:\r\n    if(this.depart != \"\" && this.Destinaton != \"\" && this.depart != this.Destinaton )  {\r\n        axios\r\n          .get(\"http://localhost:5000/api/Villes/\" + this.depart + \"/ville\")\r\n          .then((response) => {\r\n            this.iddepart = response.data.idVille;\r\n            axios\r\n              .get(\r\n                \"http://localhost:5000/api/Villes/\" + this.Destinaton + \"/ville\"\r\n              )\r\n              .then((response) => {\r\n                this.iddestination = response.data.idVille;\r\n                axios.post(\"http://localhost:5000/api/Trajets\", {\r\n                  idVille1: this.iddepart,\r\n                  idVille2: this.iddestination,\r\n                  idCamion: localStorage.getItem('moncamion'),\r\n                  date:this.dateheure,\r\n                });\r\n\r\n              });\r\n          }).then(()=>{\r\n            this.sucess=true;\r\n\r\n          })\r\n      }\r\n       else if(this.Destinaton==this.depart && this.Destinaton!='' && this.depart!=''){\r\n        this.error2=true\r\n        this.sucess=false\r\n        console.log(this.error2)\r\n      }\r\n      \r\n    break;\r\n  case false:\r\n        if(this.depart != \"\" && this.Destinaton != \"\" && this.depart != this.Destinaton)  {\r\n      axios\r\n          .get(\"http://localhost:5000/api/Villes/\" + this.depart + \"/ville\")\r\n          .then((response) => {\r\n            this.iddepart = response.data.idVille;\r\n            axios\r\n              .get(\r\n                \"http://localhost:5000/api/Villes/\" + this.Destinaton + \"/ville\"\r\n              )\r\n              .then((response) => {\r\n                this.iddestination = response.data.idVille;\r\n                axios.post(\"http://localhost:5000/api/Trajets\", {\r\n                  idVille1: this.iddepart,\r\n                  idVille2: this.iddestination,\r\n                  idCamion: this.camion,\r\n                  date:this.dateheure,\r\n                }).