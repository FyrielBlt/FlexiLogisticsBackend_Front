,\n\n    ChangePage(NumPage) {\n      this.currentPage = NumPage;\n      this.perPage = this.perPage;\n      axios;\n      axios.get(\"http://localhost:5000/api/offres/\" + localStorage.getItem('idtransporteur') + \"/offrestransporteur\" + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&&depart=\" + this.depart + \"&arrive=\" + this.arrive + \"&date=\" + this.date + \"&etat=\" + this.etat).then(resp => {\n        this.offres = resp.data;\n      });\n    },\n\n    accepter(id) {\n      axios.put(\"http://localhost:5000/api/offres/\" + id, {\n        id: id,\n        description: \"eesfgdgdsf gfhbxdvbf\",\n        date: \"2022-04-12T00:00:00\",\n        idEtat: 2,\n        prix: \"120\",\n        idTransporteur: 1,\n        idDemande: 1,\n        recu: 1\n      }).then(() => {\n        axios.get(\"http://localhost:5000/api/offres/\" + localStorage.getItem(\"iduser\")).then(resp => {\n          this.offres = resp.data;\n        });\n      });\n    },\n\n    annuler(id) {\n      this.$swal({\n        title: \"Voulez vous annuler l'offre?\",\n        type: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#36c6d3\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Oui\",\n        cancelButtonText: \"Non\"\n      }).then(result => {\n        if (result.value) {\n          axios.put(\"http://localhost:5000/api/Offres/\" + id, {\n            idOffre: this.table.idOffre,\n            description: this.table.description,\n            date: this.offre.substr(0, 10),\n            heurdepart: this.offre.substr(20, 24),\n            idEtat: this.nondispo.idEtat,\n            prix: this.table.prix,\n            prixFinale: null,\n            idTransporteur: localStorage.getItem(\"idtransporteur\"),\n            idDemande: this.table.idDemande\n          }).then(() => {\n            this.close();\n          });\n          this.$swal(\"Annulation!\", \"Offre annulé\", \"reussi\");\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAslBA,SAASA,eAAT,QAAgC,KAAhC,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,OAAOC,sBAAP,MAAmC,2DAAnC;AACA,eAAeH,eAAe,CAAC;AAC7BI,YAAU,EAAE;AAAC;AACZD,0BADW;AAEXD;AAFW,GADiB;;AAK7BG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAEPC,YAAM,EAAC,EAFA;AAGJC,iBAAW,EAAC,CAHR;AAIJC,cAAQ,EAAC,EAJL;AAKJC,UAAI,EAAC,EALD;AAMLC,aAAO,EAAC,CANH;AAOLC,WAAK,EAAC,EAPD;AAQLC,UAAI,EAAC,EARA;AASLC,YAAM,EAAC,EATF;AAULC,YAAM,EAAC,EAVF;AAWJC,UAAI,EAAC,EAXD;AAYJC,cAAQ,EAAC;AAZL,KAAP;AAcD,GApB4B;;AAqB7BC,SAAO,GAAG;AACPjB,SAAI,CACFkB,GADF,CAEG,4DAFH,EAIEC,IAJF,CAIQC,QAAD,IAAa;AACjB,WAAKJ,QAAL,GAAeI,QAAQ,CAAChB,IAAxB;AACD,KANF;AAOAJ,SAAI,CACFkB,GADF,CACM,kCADN,EAEEC,IAFF,CAEQE,IAAD,IAAW,KAAKf,MAAL,GAAce,IAAI,CAACjB,IAFrC;AAGAJ,SAAI,CACFkB,GADF,CAEG,6CACEI,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJL,EAMEJ,IANF,CAMQC,QAAD,IAAc;AACtBpB,WAAI,CACDkB,GADH,CACO,sCAAqCE,QAAQ,CAAChB,IAAT,CAAcoB,cAAnD,GAAkE,qBADzE,EAEGL,IAFH,CAESE,IAAD,IAAU;AACd,aAAKT,IAAL,GAAYS,IAAI,CAACjB,IAAL,CAAUqB,MAAtB;AACD,OAJH;AAKG,KAZF;AAcDzB,SAAI,CACDkB,GADH,CACO,sCAAqCI,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAArC,GAA4E,qBAA5E,GACJ,QADI,GACK,KAAKhB,WADV,GACsB,gBADtB,GACuC,KAAKG,OAFnD,EAIGS,IAJH,CAISE,IAAD,IAAU;AACd,WAAKhB,MAAL,GAAcgB,IAAI,CAACjB,IAAnB;AACD,KANH;AAQD,GAtD4B;;AAuD7BsB,SAAO,EAAE;AACPC,kBAAc,CAACC,GAAD,EAAK;AAEnB5B,WAAI,CACDkB,GADH,CACO,sCAAqCI,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAArC,GAA4E,qBAA5E,GACJ,QADI,GACK,KAAKhB,WADV,GACsB,gBADtB,GACuC,KAAKG,OAD5C,GAEJ,WAFI,GAEQ,KAAKG,MAFb,GAGJ,UAHI,GAGO,KAAKC,MAHZ,GAGoB,QAHpB,GAG6B,KAAKL,IAHlC,GAGuC,QAHvC,GAGgD,KAAKM,IAHrD,GAIJ,UAJI,GAIO,KAAKP,QALnB,EAOGW,IAPH,CAOSE,IAAD,IAAU;AACd,aAAKhB,MAAL,GAAcgB,IAAI,CAACjB,IAAnB;AACD,OATH;AAWC,KAdM;;AAeNyB,cAAU,CAACC,OAAD,EAAU;AACjB,WAAKvB,WAAL,GAAiBuB,OAAjB;AACA,WAAKpB,OAAL,GAAa,KAAKA,OAAlB;AACCV,WAAI;AAETA,WAAI,CACDkB,GADH,CACO,sCAAqCI,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAArC,GAA4E,qBAA5E,GACJ,QADI,GACK,KAAKhB,WADV,GACsB,gBADtB,GACuC,KAAKG,OAD5C,GAEJ,WAFI,GAEQ,KAAKG,MAFb,GAGJ,UAHI,GAGO,KAAKC,MAHZ