      uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                File\r\n              </th>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                Prix\r\n              </th>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                Etat\r\n              </th>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                Settings\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(u, index) in offres\" :key=\"index\">\r\n              <td class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\">\r\n                <div class=\"flex items-center\">\r\n                  <div class=\"ml-3\">\r\n                    <p class=\"text-gray-900 whitespace-nowrap\">\r\n                      {{ u.idDemandeNavigation.description }}\r\n                    </p>\r\