.then((resp) => {\n            this.total = resp.data.length;\n          });\n   axios\n          .get(\n            \"http://localhost:5000/api/FactureClients/client/\" +\n              localStorage.getItem(\"iduser\") +\n              \"?page=\" +\n              this.page +\n              \"&quantityPage=\" +\n              this.perpage\n               \n          )\n          .then((resp) => {\n            this.factures = resp.data;\n          });\n  },\n  methods:{\n        ChangePage(NumPage) {\n      this.page = NumPage;\n        axios\n          .get(\n            \"http://localhost:5000/api/FactureClients/client/\" +\n              localStorage.getItem(\"iduser\") +\n              \"?page=\" +\n              this.page +\n              \"&quantityPage=\" +\n              this.perpage\n               \n          )\n          .then((resp) => {\n            this.factures = resp.data;\n          });\n      \n    },\n\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}