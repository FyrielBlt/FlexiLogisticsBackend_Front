{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  components: {},\n\n  data() {\n    return {\n      chauffeurs: [],\n      camions: [],\n      demandes: [],\n      offretotal: [],\n      facturesp: [],\n      facturesnp: []\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/FactureClients/client/\" + localStorage.getItem(\"iduser\") + \"?etat=Payé\").then(resp => {\n      this.facturesp = resp.data.length;\n    });\n    axios.get(\"http://localhost:5000/api/FactureClients/client/\" + localStorage.getItem(\"iduser\") + \"?etat=Non payé\").then(resp => {\n      this.facturesnp = resp.data.length;\n    });\n    axios.get(\"http://localhost:5000/api/offres/\" + localStorage.getItem('idtransporteur') + \"/offrestransporteur\").then(response => {\n      this.offretotal = response.data;\n    });\n    axios.get(\"http://localhost:5000/api/Chauffeurs/\" + localStorage.getItem(\"societe\") + \"/chauffeurs\").then(response => {\n      this.chauffeurs = response.data;\n    }).catch(error => console.log(error));\n    axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n      axios.get(\"http://localhost:5000/api/Camions/\" + response.data.idTransporteur).then(response => {\n        this.camions = response.data;\n        console.log(this.camions);\n      }).catch(error => console.log(error));\n    }).catch(error => console.log(error));\n    axios.get(\"http://localhost:5000/api/DemandeDevis/\" + localStorage.getItem('idtransporteur') + \"/transporteur\").then(response => {\n      this.demandes = response.data;\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    pie() {\n      let liste = [];\n      liste.push(['Payé', this.facturesp]);\n      liste.push(['Non Payé', this.facturesnp]);\n      return liste;\n    },\n\n    timeFrom(X) {\n      var dates = [];\n\n      for (let I = 0; I < Math.abs(X); I++) {\n        dates.push(new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getFullYear() + \"-\" + (new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getMonth() + 1).toString().padStart(2, \"0\") + \"-\" + new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getDate().toString().padStart(2, \"0\"));\n      }\n\n      return dates;\n    },\n\n    web() {\n      let liste = [];\n      var j1 = this.timeFrom(7)[0];\n      var j2 = this.timeFrom(7)[1];\n      var j3 = this.timeFrom(7)[2];\n      var j4 = this.timeFrom(7)[3];\n      var j5 = this.timeFrom(7)[4];\n      var j6 = this.timeFrom(7)[5];\n      var j7 = this.timeFrom(7)[6];\n      var data = {\n        [j7]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j7).length,\n        [j6]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j6).length,\n        [j5]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j5).length,\n        [j4]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j4).length,\n        [j3]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j3).length,\n        [j2]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j2).length,\n        [j1]: this.demandes.filter(el => el.dateEnvoit.substr(0, 10) == j1).length\n      };\n      var offretotal = {\n        [j7]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j7).length,\n        [j6]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j6).length,\n        [j5]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j5).length,\n        [j4]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j4).length,\n        [j3]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j3).length,\n        [j2]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j2).length,\n        [j1]: this.offretotal.filter(el => el.datecreation.substr(0, 10) == j1).length\n      };\n      var ob = {\n        name: \"Demande Livraison par jour\",\n        data: data\n      };\n      var ob2 = {\n        name: \"Offre envoyé par jour\",\n        data: offretotal\n      };\n      liste.push(ob);\n      liste.push(ob2);\n      return liste;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8EA,OAAOA,KAAP,MAAkB,OAAlB;AAGA,eAAe;AACbC,YAAU,EAAE,EADC;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE,EADP;AAELC,aAAO,EAAC,EAFH;AAGLC,cAAQ,EAAC,EAHJ;AAILC,gBAAU,EAAC,EAJN;AAKLC,eAAS,EAAC,EALL;AAMLC,gBAAU,EAAC;AANN,KAAP;AAQD,GAbY;;AAcbC,SAAO,GAAG;AACRT,SAAI,CACDU,GADH,CAEI,qDACAC,YAAY,CAACC,OAAb,CAAqB,QAArB,CADA,GAEA,YAJJ,EAMGC,IANH,CAMSC,IAAD,IAAU;AACd,WAAKP,SAAL,GAAiBO,IAAI,CAACZ,IAAL,CAAUa,MAA3B;AACD,KARH;AASAf,SAAI,CACDU,GADH,CAEI,qDACAC,YAAY,CAACC,OAAb,CAAqB,QAArB,CADA,GAEA,gBAJJ,EAMGC,IANH,CAMSC,IAAD,IAAU;AACd,WAAKN,UAAL,GAAkBM,IAAI,CAACZ,IAAL,CAAUa,MAA5B;AACD,KARH;AASAf,SAAI,CACDU,GADH,CACO,sCAAqCC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAArC,GAA4E,qBADnF,EAGGC,IAHH,CAGSG,QAAD,IAAa;AACjB,WAAKV,UAAL,GAAiBU,QAAQ,CAACd,IAA1B;AACD,KALH;AAOAF,SAAI,CACDU,GADH,CAEI,0CACEC,YAAY,CAACC,OAAb,CAAqB,SAArB,CADF,GAEE,aAJN,EAMGC,IANH,CAMSG,QAAD,IAAc;AAClB,WAAKb,UAAL,GAAkBa,QAAQ,CAACd,IAA3B;AACD,KARH,EASGe,KATH,CASUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CATpB;AAUIlB,SAAI,CACLU,GADC,CAEA,6CACEC,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJF,EAMDC,IANC,CAMKG,QAAD,IAAc;AAClBhB,WAAI,CACDU,GADH,CAEI,uCAAuCM,QAAQ,CAACd,IAAT,CAAcmB,cAFzD,EAIGR,IAJH,CAISG,QAAD,IAAc;AAClB,aAAKZ,OAAL,GAAeY,QAAQ,CAACd,IAAxB;AACAiB,eAAO,CAACC,GAAR,CAAY,KAAKhB,OAAjB;AACD,OAPH,EAQGa,KARH,CAQUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CARpB;AASD,KAhBC,EAiBDD,KAjBC,CAiBMC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAjBhB;AAmBAlB,SAAI,CACDU,GADH,CAEI,4CACEC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CADF,GAEE,eAJN,EAOGC,IAPH,CAOSG,QAAD,IAAc;AAElB,WAAKX,QAAL,GAAgBW,QAAQ,CAACd,IAAzB;AACD,KAVH,EAWGe,KAXH,CAWUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAXpB;AAaL,GAlFY;;AAmFbI,SAAO,EAAE;AACPC,OAAG,GAAG;AACJ,UAAIC,KAAI,GAAI,EAAZ;AACAA,WAAK,CAACC,IAAN,CAAW,CAAC,MAAD,EAAQ,KAAKlB,SAAb,CAAX;AACAiB,WAAK,CAACC,IAAN,CAAW,CA