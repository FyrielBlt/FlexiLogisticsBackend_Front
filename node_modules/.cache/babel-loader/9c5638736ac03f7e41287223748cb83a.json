tFullYear() + \"-0\" + (new Date().getMonth() + 1) + \"-\" + new Date().getDate()\n      }).then(resp => {\n        if (this.imageFile != null) {\n          for (let i = 0; i < this.imageFile.length; i++) {\n            let file = new FormData();\n            file.append(\"idOffre\", resp.data.idOffre);\n            file.append(\"nomFile\", \"\");\n            file.append(\"imageFile\", this.imageFile[i]);\n            file.append(\"srcOffreFile\", \"\");\n            axios.post(url + \"FileOffres\", file).then(() => {\n              if (this.imageFile.length - 1 === i) {\n                axios.put(url + \"DemandeDevis/\" + this.tabledemande.idDemandeDevis, {\n                  idDemandeDevis: this.tabledemande.idDemandeDevis,\n                  dateEnvoit: this.tabledemande.dateEnvoit,\n                  idIntermediaire: this.tabledemande.idIntermediaire,\n                  idDemande: this.tabledemande.idDemande,\n                  idTransporteur: this.tabledemande.idTransporteur,\n                  idEtat: this.accepte\n                }).then(() => {\n                  this.$swal({\n                    position: \"top-end\",\n                    icon: \"success\",\n                    toast: true,\n                    title: \"Offre ajouté\",\n                    showConfirmButton: false,\n                    timer: 2000\n                  });\n                  setTimeout(location.reload(), 2000);\n                });\n              }\n            });\n          }\n        } else {\n          axios.put(url + \"DemandeDevis/\" + this.tabledemande.idDemandeDevis, {\n            idDemandeDevis: this.tabledemande.idDemandeDevis,\n            dateEnvoit: this.tabledemande.dateEnvoit,\n            idIntermediaire: this.tabledemande.idIntermediaire,\n            idDemande: this.tabledemande.idDemande,\n            idTransporteur: this.tabledemande.idTransporteur,\n            idEtat: this.accepte\n          }).then(() => {\n            this.$swal({\n              position: \"top-end\",\n              icon: \"success\",\n              toast: true,\n              title: \"Offre ajouté\",\n              showConfirmButton: false,\n              timer: 2000\n            });\n            setTimeout(location.reload(), 2000);\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,oBAAhB;AAEA,eAAe;AACbC,OAAK,EAAE,CAAC,WAAD,EAAc,cAAd,CADM;;AAGbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,KAAKC,SADP;AAELC,iBAAW,EAAE,EAFR;AAGLC,kBAAY,EAAE,EAHT;AAILC,YAAM,EAAE,EAJH;AAKLC,UAAI,EAAE,KALD;AAMLC,kBAAY,EAAE,EANT;AAOLC,eAAS,EAAE,IAPN;AAQLC,eAAS,EAAE,EARN;AASLC,YAAM,EAAE,KATH;AAULC,WAAK,EAAE,KAVF;AAWLC,cAAQ,EAAE,CAXL;AAYLC,uBAAiB,EAAE,EAZd;AAaLC,oBAAc,EAAE,EAbX;AAcLC,OAAC,EAAE,CAdE;AAeLC,iBAAW,EAAE,EAfR;AAgBLC,UAAI,EAAE,EAhBD;AAiBLC,WAAK,EAAE,EAjBF;AAkBLC,aAAO,EAAE,EAlBJ;AAmBLC,aAAO,EAAE,EAnBJ;AAoBLC,WAAK,EAAE,IAAIC,IAAJ,GAAWC,WAAX,GAAyBC,MAAzB,CAAgC,CAAhC,EAAmC,EAAnC,IAAyC,eApB3C;AAqBLC,aAAO,EAAE;AArBJ,KAAP;AAuBD,GA3BY;;AA4BbC,SAAO,GAAG;AACR;AACA7B,SAAI,CACD8B,GADH,CAEI7B,GAAE,GAAI,gDAFV,EAIG8B,IAJH,CAISC,QAAD,IAAc;AAClB,WAAKT,OAAL,GAAeS,QAAQ,CAAC7B,IAAT,CAAc8B,MAA7B;AACD,KANH,GAOE;AACAjC,SAAI,CACD8B,GADH,CACO7B,GAAE,GAAI,kCADb,EAEG8B,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKV,OAAL,GAAeU,QAAQ,CAAC7B,IAAT,CAAc8B,MAA7B;AACD,KAJH,CARF;AAaD,GA3CY;;AA4CbC,SAAO,EAAE;AACPC,SAAK,GAAG;AACN,WAAKrB,KAAL,GAAa,KAAb;AACA,WAAKD,MAAL,GAAc,KAAd;AACA,WAAKJ,IAAL,GAAY,KAAZ;AACA2B,cAAQ,CAACC,MAAT;AAED,KAPM;;AAQPC,gBAAY,CAACC,KAAD,EAAQ;AAClB,WAAK5B,SAAL,GAAiB4B,KAAK,CAACC,MAAN,CAAaC,KAA9B;AACAC,aAAO,CAACC,GAAR,CAAY,KAAKhC,SAAL,CAAe,CAAf,CAAZ;AACA+B,aAAO,CAACC,GAAR,CAAY,KAAKhC,SAAL,CAAe,CAAf,CAAZ;AACD,KAZM;;AAaPiC,gBAAY,GAAG;AACb,WAAKhB,OAAL,GAAe,IAAf;AACA5B,WAAI,CACD6C,IADH,CACQ5C,GAAE,GAAI,QADd,EACwB;AACpBkB,mBAAW,EAAE,KAAKA,WADE;AAEpB2B,YAAI,EAAE,KAAKzB,KAAL,CAAW0B,QAAX,GAAsBpB,MAAtB,CAA6B,CAA7B,EAAgC,EAAhC,CAFc;AAGpBqB,mBAAW,EAAE,KAAK3B,KAAL,CAAW0B,QAAX,GAAsBpB,MAAtB,CAA6B,EAA7B,EAAiC,EAAjC,CAHO;AAIpBM