{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport BreadCrumb from \"../../../components/Intermediaire/BreadCrumb.vue\";\nimport axios from \"axios\";\nimport Url from \"../../../store/Api\";\nimport PaginationVue from \"../../../components/Intermediaire/pagination/PaginationVue.vue\";\nexport default {\n  components: {\n    BreadCrumb,\n    PaginationVue\n  },\n\n  data() {\n    return {\n      ParpageFactureClients: \"1\",\n      livrer: \"\",\n      achever: \"\",\n      order: \"0\",\n      recherche: \"\"\n    };\n  },\n\n  async created() {\n    await axios.get(Url + \"EtatDemandeLivraisons\", {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      this.$store.dispatch(\"Get_EtatFacture\", res.data);\n      this.livrer = res.data.filter(el => el.etatDemande == \"Livré\").map(el => el.idEtatDemande)[0];\n      this.achever = res.data.filter(el => el.etatDemande == \"Achevé\").map(el => el.idEtatDemande)[0];\n    });\n    await axios.get(Url + `DemandeLivraisons?page=${this.currentFacture}&quantityPage=${this.parPageFacture}&filter=${this.livrer}&filter1=${this.achever}`, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      this.$store.dispatch(\"Get_Facture\", res);\n    });\n  },\n\n  computed: { ...mapGetters([\"ListeEtatOffres\", \"ListeFactures\", \"ListeEtatDemandeLivraisons\", \"currentFacture\", \"perPageFacture\", \"parPageFacture\", \"totalFacture\"])\n  },\n\n  mounted() {\n    this.$store.dispatch(\"Get_EtatDemandeLivraison\");\n    this.$store.dispatch(\"Get_EtatOffre\");\n  },\n\n  methods: {\n    facture(id, listfacture) {\n      //alert(listfacture[0].idFactClient +\"\" + id);\n      if (listfacture.length == 0) {\n        this.$router.push({\n          name: \"UploadFacture\",\n          params: {\n            demande: id,\n            idFactClient: 0\n          }\n        });\n      } else {\n        this.$router.push({\n          name: \"UploadFacture\",\n          params: {\n            demande: id,\n            idFactClient: listfacture[0].idFactClient\n          }\n        });\n      }\n    },\n\n    Payement(id, listfacture) {\n      let idfacture = listfacture.filter(el => el.idDemandeLivraison == id).map(el => el.idFactClient)[0];\n      this.$router.push({\n        name: \"PayementClient\",\n        params: {\n          id: idfacture\n        }\n      });\n    },\n\n    VerifPayementClient(id, listfacture) {\n      return listfacture.filter(el => el.idDemandeLivraison == id).map(el => el.payementFile)[0];\n    },\n\n    PrixFinale(indice) {\n      let prix = this.ListeFactures[indice].offre.filter(el => el.idEtat == this.GetEtatAccepter()).map(el => el.prixFinale)[0];\n      return prix;\n    },\n\n    Prix(indice) {\n      let prix = this.ListeFactures[indice].offre.filter(el => el.idEtat == this.GetEtatAccepter()).map(el => el.prix)[0];\n      return prix;\n    },\n\n    GetEtatAccepter() {\n      return this.ListeEtatOffres.filter(el => el.etat == \"Accepté\").map(el => el.idEtat)[0];\n    },\n\n    GetEtatDemande(id) {\n      return this.ListeEtatDemandeLivraisons.filter(el => el.idEtatDemande == id).map(el => el.etatDemande)[0];\n    },\n\n    DemandeDevis(id) {\n      this.$router.push({\n        name: \"DemandeDevis\",\n        params: {\n          id: id\n        }\n      });\n    },\n\n    Offre(id) {\n      this.$router.push({\n        name: \"OffreIntermediaire\",\n        params: {\n          id: id\n        }\n      });\n    },\n\n    ListeOffre(id) {\n      this.$router.push({\n        name: \"ListeOffre\",\n        params: {\n          id: id\n        }\n      });\n    },\n\n    ChangePage(NumPage) {\n      this.$store.dispatch(\"Get_NoveauFacture\", NumPage);\n    }\n\n  },\n  watch: {\n    ParpageFactureClients() {\n      this.$store.dispatch(\"Changer_ParpageFactureClients\", this.ParpageFactureClients);\n    },\n\n    order() {\n      this.$store.dispatch(\"Chercher_order\", this.order);\n    },\n\n    recherche() {\n      this.$store.dispatch(\"Chercher\", this.recherche);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkfA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,UAAP,MAAuB,kDAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,oBAAhB;AACA,OAAOC,aAAP,MAA0B,gEAA1B;AACA,eAAe;AACbC,YAAU,EAAE;AACVJ,cADU;AAEVG;AAFU,GADC;;AAKbE,MAAI,GAAG;AACL,WAAO;AACLC,2BAAqB,EAAE,GADlB;AAELC,YAAM,EAAE,EAFH;AAGLC,aAAO,EAAE,EAHJ;AAILC,WAAK,EAAC,GAJD;AAKLC,eAAS,EAAC;AALL,KAAP;AAOD,GAbY;;AAcb,QAAMC,OAAN,GAAgB;AACd,UAAMV,KAAI,CACPW,GADG,CACCV,GAAE,GAAI,uBADP,EACgC;AAClCW,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AADyB,KADhC,EAMHC,IANG,CAMGC,GAAD,IAAS;AACb,WAAKC,MAAL,CAAYC,QAAZ,CAAqB,iBAArB,EAAwCF,GAAG,CAACb,IAA5C;AACA,WAAKE,MAAL,GAAcW,GAAG,CAACb,IAAJ,CACXgB,MADW,CACHC,EAAD,IAAQA,EAAE,CAACC,WAAH,IAAkB,OADtB,EAEXC,GAFW,CAENF,EAAD,IAAQA,EAAE,CAACG,aAFJ,EAEmB,CAFnB,CAAd;AAGA,WAAKjB,OAAL,GAAeU,GAAG,CAACb,IAAJ,CACZgB,MADY,CACJC,EAAD,IAAQA,EAAE,CAACC,WAAH,IAAkB,QADrB,EAEZC,GAFY,CAEPF,EAAD,IAAQA,EAAE,CAACG,aAFH,EAEkB,CAFlB,CAAf;AAGD,KAdG,CAAN;AAeA,UAAMxB,KAAI,CACPW,GADG,CAEFV,GAAE,GACC,0BAAyB,KAAKwB,cAAe,iBAAgB,KAAKC,cAAe,WAAU,KAAKpB,MAAO,YAAW,KAAKC,OAAQ,EAHhI,EAIF;AACEK,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AADX,KAJE,EAUHC,IAVG,CAUGC,GAAD,IAAS;AACb,WAAKC,MAAL,CAAYC,QAAZ,CAAqB,aAArB,EAAoCF,GAApC;AACD,KAZG,CAAN;AAaD,GA3CY;;AA4CbU,UAAQ,EAAE,EACR,GAAG7B,UAAU,CAAC,CACZ,iBADY,EAEZ,eAFY,EAGZ,4BAHY,EAIZ,gBAJY,EAKZ,gBALY,EAMZ,gBANY,EAOZ,cAPY,CAAD;AADL,GA5CG;;AAuDb8B,SAAO,GAAG;AACR,SAAKV,MAAL,CAAYC,QAAZ,CAAqB,0BAArB;AACA,SAAKD,MAAL,CAAYC,QAAZ,CAAqB,eAArB;AACD,GA1DY;;AA2DbU,SAAO,EAAE;AACPC,WAAO,CAACC,EAAD,EAAKC,WAAL,EAAkB;AACvB;AACA,UAAIA,WAAW,CAACC,MAAZ,IAAsB,CAA1B,EAA6B;AAC3B,aAAKC,OAAL,CAAaC,IAAb,CAAkB;AAChBC,cAAI,EAAE,eADU;AAEhBC,gBAAM,EAAE;AACNC,mBAAO,EAAEP,EADH;AAENQ,wBAAY,EAAE;AAFR;AAFQ,SAAlB;AAOF,OARA,MAQO;AACL,aAAKL,OAAL,CAAaC,IAAb,CAAkB;AAChBC,cAAI,EAAE,eADU;AA