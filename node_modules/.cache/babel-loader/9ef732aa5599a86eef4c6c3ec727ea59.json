 \"axios\";\nimport BreadCrumb from \"../../components/Intermediaire/BreadCrumb.vue\";\n\nimport PaginationVue from \"../../components/Intermediaire/pagination/PaginationVue.vue\";\nimport Dragdropfile from \"./components/dragdropfile.vue\";\nexport default {\n  components: {\n    //  Banner,\n    BreadCrumb,\n    PaginationVue,\n    Dragdropfile\n},\n  data() {\n    return {\n      page: 1,\n      perpage: 3,\n      factures: []\n    }\n  },\n  created() {\n   axios\n          .get(\n            \"http://localhost:5000/api/FactureClients/client/\" +\n              localStorage.getItem(\"iduser\") +\n              \"?page=\" +\n              this.page +\n              \"&quantityPage=\" +\n              this.perpage\n               \n          )\n          .then((resp) => {\n            this.factures = resp.data;\n          });\n  },\n  methods:{\n        ChangePage(NumPage) {\n      this.page = NumPage;\n        axios\n          .get(\n            \"http://localhost:5000/api/FactureClients/client/\" +\n              localStorage.getItem(\"iduser\") +\n              \"?page=\" +\n              this.page +\n              \"&quantityPage=\" +\n              this.perpage\n               \n          )\n          .then((resp) => {\n            this.factures = resp.data;\n          });\n      \n    },\n\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}