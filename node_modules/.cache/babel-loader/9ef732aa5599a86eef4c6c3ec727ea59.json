{"ast":null,"code":"import axios from \"axios\";\nimport BreadCrumb from \"../../components/Intermediaire/BreadCrumb.vue\";\nimport PaginationVue from \"../../components/Intermediaire/pagination/PaginationVue.vue\";\nimport Dragdropfile from \"./components/dragdropfile.vue\";\nexport default {\n  components: {\n    //  Banner,\n    BreadCrumb,\n    PaginationVue,\n    Dragdropfile\n  },\n\n  data() {\n    return {\n      page: 1,\n      perpage: 3,\n      factures: []\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/FactureClients/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage).then(resp => {\n      this.factures = resp.data;\n    });\n  },\n\n  methods: {\n    ChangePage(NumPage) {\n      this.page = NumPage;\n      axios.get(\"http://localhost:5000/api/FactureClients/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage).then(resp => {\n        this.factures = resp.data;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiMA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,+CAAvB;AAEA,OAAOC,aAAP,MAA0B,6DAA1B;AACA,OAAOC,YAAP,MAAyB,+BAAzB;AACA,eAAe;AACbC,YAAU,EAAE;AACV;AACAH,cAFU;AAGVC,iBAHU;AAIVC;AAJU,GADC;;AAObE,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,CADD;AAELC,aAAO,EAAE,CAFJ;AAGLC,cAAQ,EAAE;AAHL,KAAP;AAKD,GAbY;;AAcbC,SAAO,GAAG;AACTT,SAAI,CACIU,GADR,CAES,qDACEC,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKN,IAHP,GAIE,gBAJF,GAKE,KAAKC,OAPhB,EAUQM,IAVR,CAUcC,IAAD,IAAU;AACd,WAAKN,QAAL,GAAgBM,IAAI,CAACT,IAArB;AACD,KAZR;AAaA,GA5BY;;AA6BbU,SAAO,EAAC;AACFC,cAAU,CAACC,OAAD,EAAU;AACtB,WAAKX,IAAL,GAAYW,OAAZ;AACEjB,WAAI,CACDU,GADH,CAEI,qDACEC,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKN,IAHP,GAIE,gBAJF,GAKE,KAAKC,OAPX,EAUGM,IAVH,CAUSC,IAAD,IAAU;AACd,aAAKN,QAAL,GAAgBM,IAAI,CAACT,IAArB;AACD,OAZH;AAcH;;AAjBK;AA7BK,CAAf","names":["axios","BreadCrumb","PaginationVue","Dragdropfile","components","data","page","perpage","factur