{"ast":null,"code":"export default {\n  props: [\"demandes\"],\n\n  data() {\n    const month = new Date().getMonth();\n    const year = new Date().getFullYear();\n    return {\n      masks: {\n        weekdays: 'WWW'\n      },\n      attributes: [{\n        customData: {\n          title: 'Lunch with mom.',\n          class: 'bg-red-600 text-white'\n        },\n        dates: new Date(year, month, 1)\n      }]\n    };\n  },\n\n  methods: {\n    calendardata() {\n      let list = [];\n      this.demandes.forEach(element => {\n        let ob = {\n          customData: {\n            title: element.idDemandeDevis,\n            class: 'bg-slate-500 font-semibold text-white'\n          },\n          dates: element.dateEnvoit\n        };\n        list.push(ob);\n      });\n      return list;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoCA,eAAe;AACbA,OAAK,EAAC,CAAC,UAAD,CADO;;AAEbC,MAAI,GAAG;AACL,UAAMC,KAAI,GAAI,IAAIC,IAAJ,GAAWC,QAAX,EAAd;AACA,UAAMC,IAAG,GAAI,IAAIF,IAAJ,GAAWG,WAAX,EAAb;AACA,WAAO;AACLC,WAAK,EAAE;AACLC,gBAAQ,EAAE;AADL,OADF;AAILC,gBAAU,EAAE,CACV;AACEC,kBAAU,EAAE;AACVC,eAAK,EAAE,iBADG;AAEVC,eAAK,EAAE;AAFG,SADd;AAKEC,aAAK,EAAE,IAAIV,IAAJ,CAASE,IAAT,EAAeH,KAAf,EAAsB,CAAtB;AALT,OADU;AAJP,KAAP;AAeD,GApBY;;AAqBbY,SAAO,EAAC;AACNC,gBAAY,GAAE;AACZ,UAAIC,IAAG,GAAI,EAAX;AACA,WAAKC,QAAL,CAAcC,OAAd,CAAsBC,OAAM,IAAK;AAC/B,YAAIC,EAAC,GAAI;AACTV,oBAAU,EAAE;AACRC,iBAAK,EAAEQ,OAAO,CAACE,cADP;AAERT,iBAAK,EAAE;AAFC,WADH;AAKPC,eAAK,EAAEM,OAAO,CAACG;AALR,SAAT;AAOFN,YAAI,CAACO,IAAL,CAAUH,EAAV;AACC,OATD;AAWA,aAAOJ,IAAP;AACF;;AAfM;AArBK,CAAf","names":["props","data","month","Date","getMonth","year","getFullYear","masks","weekdays","attributes","customData","title","class","dates","methods","calendardata","list","demandes","forEach","element","ob","idDemandeDevis","dateEnvoit","push"],"sourceRoot":"","sources":["C:\\Users\\Karim Omrane\\Desktop\\PFE FI\\FRONT\\src\\views\\Transporteur\\Users\\v-calendar.vue"],"sourcesContent":["<template>\n  <div class=\"text-center section\" style=\"width: 695px;\n    height: 210px;\n    \">\n    <p class=\"text-lg font-medium text-gray-600 mb-6\" style=\"background-color: white;\n    position: relative;\n    top: 15px;\">\n      Calendrier des demandes\n    </p>\n    <v-calendar\n      class=\"custom-calendar max-w-full\"\n      :masks=\"masks\"\n      :attributes=\"calendardata()\"\n      disable-page-swipe\n      is-expanded\n    >\n      <template v-slot:day-content=\"{ day, attributes }\">\n        <div class=\"flex flex-col h-20 z-10 overflow-hidden\">\n          <span class=\"day-label text-sm text-gray-900\">{{ day.day }}</span>\n          <div class=\"flex-grow  overflow-y-auto overflow-x-auto\">\n            <p\n              v-for=\"attr in attributes\"\n              :key=\"attr.key\"\n              class=\"text-xs leading-tight rounded-sm p-1 mt-0 mb-1\"\n              :class=\"attr.customData.class\"\n            >\n              {{ attr.customData.title }}\n            </p>\n          </div>\n        </div>\n      </template>\n    </v-calendar>\n  </div>\n</template>\n\n<script>\nexport default {\n  props:[\"demandes\"],\n  data() {\n    const month = new Date().getMonth();\n    const year = new Date().getFullYear();\n    return {\n      masks: {\n        weekdays: 'WWW',\n      },\n      attributes: [\n        {\n          customData: {\n            title: 'Lunch with mom.',\n            class: 'bg-red-600 text-white',\n          },\n          dates: new Date(year, month, 1),\n        },\n        \n      ],\n    };\n  },\n  methods:{\n    calendardata(){\n      let list = [];\n      this.demandes.forEach(element => {\n        let ob = {\n        customData: {\n            title: element.idDemandeDevis,\n            class: 'bg-slate-500 font-semibold text-white',\n          },\n          dates: element.dateEnvoit,\n      }\n      list.push(ob)\n      });\n      \n      return list\n    }\n  }\n};\n</script>\n\n<style lang=\"postcss\" scoped>\n::-webkit-scrollbar {\n  width: 0px;\n}\n::-webkit-scrollbar-track {\n  display: none;\n}\n/deep/ .custom-calendar.vc-container {\n  --day-border: 1px solid #b8c2cc;\n  --day-border-highlight: 1px solid #b8c2cc;\n  --day-width: 90px;\n  --day-height: 90px;\n  --weekday-bg: #f8fafc;\n  --weekday-border: 1px solid #eaeaea;\n  border-radius: 0;\n  width: 100%;\n  & .vc-header {\n    background-color: #f1f5f8;\n    padding: 10px 0;\n  }\n  & .vc-weeks {\n    padding: 0;\n  }\n  & .vc-weekday {\n    background-color: var(--weekday-bg);\n    border-bottom: var(--weekday-border);\n    border-top: var(--weekday-border);\n    padding: 5px 0;\n  }\n  & .vc-day {\n    padding: 0 5px 3px 5px;\n    text-align: left;\n    height: var(--day-height);\n    min-width: var(--day-width);\n    background-color: white;\n    &.weekday-1,\n    &.weekday-7 {\n      background-color: #eff8ff;\n    }\n    &:not(.on-bottom) {\n      border-bottom: var(--day-border);\n      &.weekday-1 {\n        border-bottom: var(--day-border-highlight);\n      }\n    }\n    &:not(.on-right) {\n      border-right: var(--day-border);\n    }\n  }\n  & .vc-day-dots {\n    margin-bottom: 5px;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}