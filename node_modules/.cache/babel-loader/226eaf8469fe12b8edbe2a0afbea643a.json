{"ast":null,"code":"import axios from \"axios\";\nimport Url from \"../../../store/Api\";\nimport { mapGetters } from \"vuex\";\nimport BreadCrumb from \"../../../components/Intermediaire/BreadCrumb.vue\"; // import PaginationVue from \"../../../components/Intermediaire/pagination/PaginationVue.vue\";\n\nexport default {\n  components: {\n    BreadCrumb // PaginationVue,\n\n  },\n\n  data() {\n    return {\n      //listeOffre: [],\n      checkedOffreId: []\n    };\n  },\n\n  computed: { ...mapGetters([\"ListeEtatOffres\", \"ListeOffres\"])\n  },\n\n  mounted() {\n    this.$store.dispatch(\"Get_EtatOffre\");\n  },\n\n  async created() {\n    await axios.get(Url + \"DemandeLivraisons/\" + this.$route.params.id, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      console.log(res.data); // this.checkedOffreId = res.data[0].offre\n      //   .filter((el) => el.idEtat == this.GetEtatAccepter())\n      //   .map((el) => el.idOffre);\n\n      this.$store.dispatch(\"Get_ListeOffre\", res.data[0].offre);\n    });\n  },\n\n  methods: {\n    GetEtatEnAttente() {\n      return this.ListeEtatOffres.filter(el => el.etat == \"En attente d'acceptation\").map(el => el.idEtat)[0];\n    },\n\n    GetEtatRefuser() {\n      return this.ListeEtatOffres.filter(el => el.etat == \"Refusé\").map(el => el.idEtat)[0];\n    },\n\n    GetEtatAccepter() {\n      return this.ListeEtatOffres.filter(el => el.etat == \"Accepté\").map(el => el.idEtat)[0];\n    },\n\n    GetEtatEnCours() {\n      return this.ListeEtatOffres.filter(el => el.etat == \"En cours de traitement\").map(el => el.idEtat)[0];\n    },\n\n    Accepter() {\n      var AccepterOffre = {\n        idEtatEnCours: this.GetEtatEnCours(),\n        idEtatRefuser: this.GetEtatRefuser(),\n        ListeOffresId: this.checkedOffreId\n      };\n      this.$store.dispatch(\"Accepter_Offre\", AccepterOffre);\n    },\n\n    AnnulerAccepter(id, index) {\n      var Annuleraccepter = {\n        idOffre: id,\n        idEtatRefuser: this.GetEtatRefuser(),\n        index: index\n      }; //console.log(id)\n\n      console.log(Annuleraccepter);\n      this.$store.dispatch(\"Annuler_Accepter\", Annuleraccepter);\n    },\n\n    Valider(id, index) {\n      var AccepterValider = {\n        idOffre: id,\n        idEtatAccepter: this.GetEtatAccepter(),\n        idEtatRefuser: this.GetEtatRefuser(),\n        index: index\n      }; //console.log(id)\n\n      console.log(AccepterValider);\n      this.$store.dispatch(\"Valider_Accepter\", AccepterValider);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwZA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,oBAAhB;AACA,SAASC,UAAT,QAA2B,MAA3B;AACA,OAAOC,UAAP,MAAuB,kDAAvB,C,CACA;;AACA,eAAe;AACbC,YAAU,EAAE;AACVD,cADU,CAEV;;AAFU,GADC;;AAKbE,MAAI,GAAG;AACL,WAAO;AACL;AACAC,oBAAc,EAAE;AAFX,KAAP;AAID,GAVY;;AAYbC,UAAQ,EAAE,EACR,GAAGL,UAAU,CAAC,CAAC,iBAAD,EAAoB,aAApB,CAAD;AADL,GAZG;;AAebM,SAAO,GAAG;AACR,SAAKC,MAAL,CAAYC,QAAZ,CAAqB,eAArB;AACD,GAjBY;;AAkBb,QAAMC,OAAN,GAAgB;AACd,UAAMX,KAAI,CACPY,GADG,CACCX,GAAE,GAAI,oBAAN,GAA6B,KAAKY,MAAL,CAAYC,MAAZ,CAAmBC,EADjD,EACqD;AACvDC,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AAD8C,KADrD,EAMHC,IANG,CAMGC,GAAD,IAAS;AACbC,aAAO,CAACC,GAAR,CAAYF,GAAG,CAAChB,IAAhB,EADa,CAEb;AACA;AACA;;AAEA,WAAKI,MAAL,CAAYC,QAAZ,CAAqB,gBAArB,EAAuCW,GAAG,CAAChB,IAAJ,CAAS,CAAT,EAAYmB,KAAnD;AACD,KAbG,CAAN;AAcD,GAjCY;;AAkCbC,SAAO,EAAE;AACPC,oBAAgB,GAAE;AACf,aAAO,KAAKC,eAAL,CAAqBC,MAArB,CAA6BC,EAAD,IAAQA,EAAE,CAACC,IAAH,IAAW,0BAA/C,EAA2EC,GAA3E,CACLF,EAAD,IAAQA,EAAE,CAACG,MADL,EAEN,CAFM,CAAP;AAGF,KALM;;AAMPC,kBAAc,GAAG;AACf,aAAO,KAAKN,eAAL,CAAqBC,MAArB,CAA6BC,EAAD,IAAQA,EAAE,CAACC,IAAH,IAAW,QAA/C,EAAyDC,GAAzD,CACJF,EAAD,IAAQA,EAAE,CAACG,MADN,EAEL,CAFK,CAAP;AAGD,KAVM;;AAWPE,mBAAe,GAAG;AAChB,aAAO,KAAKP,eAAL,CAAqBC,MAArB,CAA6BC,EAAD,IAAQA,EAAE,CAACC,IAAH,IAAW,SAA/C,EAA0DC,GAA1D,CACJF,EAAD,IAAQA,EAAE,CAACG,MADN,EAEL,CAFK,CAAP;AAGD,KAfM;;AAgBPG,kBAAc,GAAG;AACf,aAAO,KAAKR,eAAL,CAAqBC,MAArB,CACJC,EAAD,IAAQA,EAAE,CAACC,IAAH,IAAW,wBADd,EAELC,GAFK,CAEAF,EAAD,IAAQA,EAAE,CAACG,MAFV,EAEkB,CAFlB,CAAP;AAGD,KApBM;;AAqBPI,YAAQ,GAAG;AACT,UAAIC,aAAY,GAAI;AAClBC,qBAAa,EAAE,KAAKH,cAAL,EADG;AAElBI,qBAAa,EAAE,KAAKN,cAAL,EAFG;AAGlBO,qBAAa,EAAE,KAAKlC;AAHF,OAApB;AAKA,WAAKG,MAAL,CAAYC,QAAZ,CAAqB,gBAArB,EAAuC2B,aAAvC;AACD,KA5BM;;AA6BPI,mBAAe,CAAC1B,EAAD,EAAI2B,KAAJ,EAAW;AACvB,UAAIC,eAAc,GAAI;AACpBC,eAAO,EAAC7B,EADY;AAEpBwB,qBAAa,EAAE,KAAKN,cAAL,EAFK;AAGpBS,aAAK,EAAEA;AAHa,OAAtB,CADuB,CAOxB;;AACApB,aAAO,CAACC,GAAR,CAAYoB,eAAZ;AACA,WAAKlC,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCiC,eAAzC;AACD,KAvCM;;AAwCPE,WAAO,CAAC9B,EAAD,EAAI2B,KAAJ,EAAW;AACf,UAAII,eAAc,GAAI;AACpBF,eAAO,EAAC7B,EADY;AAEpBgC,sBAAc,EAAE,KAAKb,eAAL,EAFI;AAGpBK,qBAAa,EAAE,KAAKN,cAAL,EAHK;AAIpBS,aAAK,EAAEA;AAJa,OAAtB,CADe,CAQhB;;AACApB,aAAO,CAACC,GAAR,CAAYuB,eAAZ;AACA,WAAKrC,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyCoC,eAAzC;AACD;;AAnDM;AAlCI,CAAf","names":["axios","Url","mapGetters","BreadCrumb","components","data","checkedOffreId","computed","mounted","$store","dispatch","created","get","$route","params","id","headers","Authorization","localStorage","getItem","then","res","console","log","offre","methods","GetEtatEnAttente","ListeEtatOffres","filter","el","etat","map","idEtat","GetEtatRefuser","GetEtatAccepter","GetEtatEnCours","Accepter","AccepterOffre","idEtatEnCours","idEtatRefuser","ListeOffresId","AnnulerAccepter","index","Annuleraccepter","idOffre","Valider","AccepterValider","idEtatAccepter"],"sourceRoot":"","sources":["C:\\Users\\Karim Omrane\\Desktop\\PFE FI\\FRONT\\src\\views\\Intermediaire\\demande\\Offre.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Breadcrumb -->\n    <!-- <Breadcrumb :breadcrumbName=\"Tables\" /> -->\n    <bread-crumb>\n      <template v-slot:bread1> Demandes </template>\n      <template v-slot:bread> Offres recus </template>\n    </bread-crumb>\n\n    <!-- <div class=\"mt-4\"> -->\n\n    <!--  -->\n\n    <div class=\"mt-8\">\n      <!-- <h4 class=\"text-gray-600\">Les Clients</h4> -->\n\n      <div class=\"mt-6\">\n        <h2 class=\"text-xl font-semibold leading-tight text-gray-700\">\n          Les Offres :\n        </h2>\n\n        <div class=\"px-4 py-4 -mx-4 overflow-x-auto sm:-mx-8 sm:px-8\">\n          <div\n            class=\"inline-block min-w-full overflow-hidden rounded-lg shadow\"\n          >\n            <table class=\"min-w-full leading-normal\">\n              <thead>\n                <tr>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    ID\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Transporteur\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Description\n                  </th>\n\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Date\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Prix\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Prix Final\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Settings\n                  </th>\n                </tr>\n              </thead>\n              <!-- liste transporteur -->\n              <tbody class=\"bg-white\">\n                <tr v-for=\"(offre, index) in ListeOffres\" :key=\"offre.idOffre\">\n                  <th\n                    class=\"px-6 py-4 border-b border-gray-200 whitespace-nowrap\"\n                  >\n                    {{ index + 1 }}\n                  </th>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                      text-center\n                    \"\n                  >\n                    <div class=\"ml-4\">\n                      <div\n                        class=\"\n                          text-sm\n                          font-medium\n                          leading-5\n                          text-center text-gray-900\n                        \"\n                      >\n                        {{\n                          offre.idTransporteurNavigation.idUserNavigation.prenom\n                        }}\n                      </div>\n                      <div class=\"text-sm leading-5 text-gray-500\">\n                        {{\n                          offre.idTransporteurNavigation.idUserNavigation.nom\n                        }}\n                      </div>\n                    </div>\n                  </td>\n\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      {{ offre.description }}\n                    </div>\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      {{ offre.date }}\n                    </div>\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      leading-5\n                      text-center text-gray-500\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    {{ offre.prix }}\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      leading-5\n                      text-center text-gray-500\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    {{ offre.prixFinale }}\n                  </td>\n<!-- settings -->\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      font-medium\n                      leading-5\n                      text-center\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-green-400\n                        rounded-r rounded-l\n                      \"\n                      v-if=\"GetEtatEnCours() == offre.idEtat\"\n                    >\n                      En cour de traitemment\n                    </button>\n                    <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-red-400\n                        rounded-r rounded-l\n                        hover:bg-red-500\n                      \"\n                      v-else-if=\"GetEtatRefuser() == offre.idEtat\"\n                    >\n                      Refuser\n                    </button>\n                     <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-cyan-400\n                        rounded-r rounded-l\n                        \"\n                      v-else-if=\"GetEtatAccepter() == offre.idEtat\"\n                    >\n                      Accepter\n                    </button>\n                    <div  v-else-if=\"GetEtatEnAttente() == offre.idEtat\">\n                    <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-cyan-400\n                        rounded-r rounded-l\n                        \"\n                        @click=\"Valider(offre.idOffre,index)\" \n                    >\n                      En attente d'acceptation\n                    </button>\n                    <button\n                    style=\"margin-left:5px\"\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-red-400\n                        rounded-r rounded-l\n                        \"\n                    @click=\"AnnulerAccepter(offre.idOffre,index)\" \n                    >\n                      Annuler\n                    </button>\n                    </div>\n                    <input\n                      type=\"checkbox\"\n                      class=\"\n                        w-5\n                        h-5\n                        text-indigo-600\n                        rounded-md\n                        focus:ring-indigo-500\n                      \"\n                      :id=\"offre.idOffre\"\n                      :value=\"offre.idOffre\"\n                      v-model=\"checkedOffreId\"\n                      v-else\n                    />\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n            <!--  -->\n\n            <div class=\"p-2 bg-white rounded-md shadow-md\">\n              <div class=\"grid grid-cols-1 gap-6 sm:grid-cols-2\">\n                <div class=\"text-center\">\n                  <div\n                    class=\"\n                      flex flex-col\n                      items-center\n                      bg-white\n                      xs:flex-row xs:justify-between\n                    \"\n                  >\n                    <div class=\"inline-flex xs:mt-0\"></div>\n                  </div>\n                </div>\n                <div class=\"text-center\">\n                  <div class=\"grid grid-cols-1 gap-6 sm:grid-cols-2\">\n                    <div\n                      class=\"\n                        py-4\n                        flex flex-col\n                        items-center\n                        bg-white\n                        xs:flex-row xs:justify-between\n                      \"\n                    >\n                      <button\n                      v-if=\"checkedOffreId.length!=0\"\n                        class=\"\n                          px-6\n                          py-2\n                          font-semibold\n                          text-gray-900\n                          bg-gray-300\n                          rounded-r rounded-l\n                          hover:bg-gray-400\n                        \"\n                        @click=\"Accepter()\"\n                      >\n                        Accepter\n                      </button>\n                    </div>\n\n                    <div\n                      class=\"\n                        px-6\n                        py-4\n                        flex flex-col\n                        items-center\n                        bg-white\n                        xs:flex-row xs:justify-between\n                      \"\n                    >\n                      <router-link to=\"../demandeIntermediaire\">\n                        <button\n                          class=\"\n                            px-4\n                            py-2\n                            text-xm\n                            font-semibold\n                            text-gray-900\n                            bg-red-400\n                            rounded-r rounded-l\n                            hover:bg-red-500\n                          \"\n                        >\n                          Annuler\n                        </button>\n                      </router-link>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n            <!-- PAGINATION -->\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- les offre selecter :{{ checkedOffreId }}\n    <br />\n    {{ GetEtatEnCours() }} -->\n  </div>\n  <!-- </div> -->\n</template>\n<script>\nimport axios from \"axios\";\nimport Url from \"../../../store/Api\";\nimport { mapGetters } from \"vuex\";\nimport BreadCrumb from \"../../../components/Intermediaire/BreadCrumb.vue\";\n// import PaginationVue from \"../../../components/Intermediaire/pagination/PaginationVue.vue\";\nexport default {\n  components: {\n    BreadCrumb,\n    // PaginationVue,\n  },\n  data() {\n    return {\n      //listeOffre: [],\n      checkedOffreId: [],\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"ListeEtatOffres\", \"ListeOffres\"]),\n  },\n  mounted() {\n    this.$store.dispatch(\"Get_EtatOffre\");\n  },\n  async created() {\n    await axios\n      .get(Url + \"DemandeLivraisons/\" + this.$route.params.id, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      })\n      .then((res) => {\n        console.log(res.data);\n        // this.checkedOffreId = res.data[0].offre\n        //   .filter((el) => el.idEtat == this.GetEtatAccepter())\n        //   .map((el) => el.idOffre);\n\n        this.$store.dispatch(\"Get_ListeOffre\", res.data[0].offre);\n      });\n  },\n  methods: {\n    GetEtatEnAttente(){\n       return this.ListeEtatOffres.filter((el) => el.etat == \"En attente d'acceptation\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatRefuser() {\n      return this.ListeEtatOffres.filter((el) => el.etat == \"Refusé\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatAccepter() {\n      return this.ListeEtatOffres.filter((el) => el.etat == \"Accepté\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatEnCours() {\n      return this.ListeEtatOffres.filter(\n        (el) => el.etat == \"En cours de traitement\"\n      ).map((el) => el.idEtat)[0];\n    },\n    Accepter() {\n      var AccepterOffre = {\n        idEtatEnCours: this.GetEtatEnCours(),\n        idEtatRefuser: this.GetEtatRefuser(),\n        ListeOffresId: this.checkedOffreId,\n      };\n      this.$store.dispatch(\"Accepter_Offre\", AccepterOffre);\n    },\n    AnnulerAccepter(id,index) {\n       var Annuleraccepter = {\n         idOffre:id,\n         idEtatRefuser: this.GetEtatRefuser(),\n         index: index,\n         \n      };\n      //console.log(id)\n      console.log(Annuleraccepter)\n      this.$store.dispatch(\"Annuler_Accepter\", Annuleraccepter);\n    },\n    Valider(id,index) {\n       var AccepterValider = {\n         idOffre:id,\n         idEtatAccepter: this.GetEtatAccepter(),\n         idEtatRefuser: this.GetEtatRefuser(),\n         index: index,\n         \n      };\n      //console.log(id)\n      console.log(AccepterValider)\n      this.$store.dispatch(\"Valider_Accepter\", AccepterValider);\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}