AoDP6C,mBAAe,CAACnC,EAAD,EAAIoC,KAAJ,EAAW;AACvB,UAAIC,eAAc,GAAI;AACpBX,eAAO,EAAC1B,EADY;AAEpBiC,qBAAa,EAAE,KAAKX,mBAAL,EAFK;AAGpBc,aAAK,EAAEA;AAHa,OAAtB,CADuB,CAOxB;;AACA7B,aAAO,CAACC,GAAR,CAAY6B,eAAZ;AACA,WAAK3C,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyC0C,eAAzC;AACD,KA9DM;;AA+DPC,WAAO,CAACtC,EAAD,EAAIoC,KAAJ,EAAW;AACf,UAAIG,eAAc,GAAI;AACpBb,eAAO,EAAC1B,EADY;AAEpBwC,sBAAc,EAAE,KAAKrB,eAAL,EAFI;AAGpBc,qBAAa,EAAE,KAAKf,cAAL,EAHK;AAIpBkB,aAAK,EAAEA;AAJa,OAAtB,CADe,CAQhB;;AACA7B,aAAO,CAACC,GAAR,CAAY+B,eAAZ;AACA,WAAK7C,MAAL,CAAYC,QAAZ,CAAqB,kBAArB,EAAyC4C,eAAzC;AACD;;AA1EM;AAnCI,CAAf","names":["axios","Url","mapGetters","BreadCrumb","components","data","checkedOffreId","accepter","computed","mounted","$store","dispatch","created","get","$route","params","id","headers","Authorization","localStorage","getItem","then","res","console","log","offre","methods","GetEtatEnAttente","ListeEtatOffres","filter","el","etat","map","idEtat","GetEtatRefuser","GetEtatAccepter","GetEtatNonTraite","GetEtatEnCours","GetEtatIndisponible","Accepter","idt","ListeOffres","idOffre","idTransporteur","length","AccepterOffreSeule","idEtatEnCours","OffreId","AccepterOffre","idEtatRefuser","ListeOffresId","AnnulerAccepter","index","Annuleraccepter","Valider","AccepterValider","idEtatAccepter"],"sourceRoot":"","sources":["C:\\Users\\Karim Omrane\\Desktop\\PFE FI\\FRONT\\src\\views\\Intermediaire\\demande\\Offre.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Breadcrumb -->\n    <!-- <Breadcrumb :breadcrumbName=\"Tables\" /> -->\n    <bread-crumb>\n      <template v-slot:bread1> Demandes </template>\n      <template v-slot:bread> Offres recus </template>\n    </bread-crumb>\n\n    <!-- <div class=\"mt-4\"> -->\n\n    <!--  -->\n\n    <div class=\"mt-8\">\n      <!-- <h4 class=\"text-gray-600\">Les Clients</h4> -->\n\n      <div class=\"mt-6\">\n        <h2 class=\"text-xl font-semibold leading-tight text-gray-700\">\n          Les Offres :\n        </h2>\n\n        <div class=\"px-4 py-4 -mx-4 overflow-x-auto sm:-mx-8 sm:px-8\">\n          <div\n            class=\"inline-block min-w-full overflow-hidden rounded-lg shadow\"\n          >\n            <table class=\"min-w-full leading-normal\">\n              <thead>\n                <tr>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    ID\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Transporteur\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Description\n                  </th>\n\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Date\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Prix\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Prix Final\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Etat\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Settings\n                  </th>\n                </tr>\n              </thead>\n              <!-- liste transporteur -->\n              <tbody class=\"bg-white\">\n                <tr v-for=\"(offre, index) in ListeOffres\" :key=\"offre.idOffre\">\n                  <th\n                    class=\"px-6 py-4 border-b border-gray-200 whitespace-nowrap\"\n                  >\n                    {{ index + 1 }}\n                  </th>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                      text-center\n                    \"\n                  >\n                    <div class=\"ml-4\">\n                      <div\n                        class=\"\n                          text-sm\n                          font-medium\n                          leading-5\n                          text-center text-gray-900\n                        \"\n                      >\n                        {{\n                          offre.idTransporteurNavigation.idUserNavigation.prenom\n                        }}\n                      </div>\n                      <div class=\"text-sm leading-5 text-gray-500\">\n                        {{\n                          offre.idTransporteurNavigation.idUserNavigation.nom\n                        }}\n                      </div>\n                    </div>\n                  </td>\n\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      {{ offre.description }}\n                    </div>\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      {{ offre.date }}\n                    </div>\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      leading-5\n                      text-center text-gray-500\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    {{ offre.prix }}\n                  </td>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      leading-5\n                      text-center text-gray-500\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    {{ offre.prixFinale }}\n                  </td>\n                    <td\n                  v-if=\"offre.idEtat== GetEtatEnCours()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-yellow-300\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\"> En cour de traitement</a>\n                      </a>\n                    </div>\n                  </td>\n                  <td\n                  v-if=\"offre.idEtat==GetEtatAccepter()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-green-500\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\">Accepté</a>\n                      </a>\n                    </div>\n                  </td>\n                  <td\n                  v-if=\"offre.idEtat==GetEtatNonTraite()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-blue-500\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\">Non traité</a>\n                      </a>\n                    </div>\n                  </td>\n                   <td\n                  v-if=\"offre.idEtat==GetEtatEnAttente()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-cyan-500\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\">En attente d'acceptation</a>\n                      </a>\n                    </div>\n                  </td>\n                  <td\n                  v-if=\"offre.idEtat==GetEtatRefuser()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-red-500\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\">Refusé</a>\n                      </a>\n                    </div>\n                  </td>\n                   <td\n                  v-if=\"offre.idEtat==GetEtatIndisponible()\"\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                    <div class=\"text-sm leading-5 text-gray-900\">\n                      <a\n                        class=\"\n                          inline-flex\n                          px-2\n                          text-ms\n                          font-semibold\n                          leading-5\n                          text-black-800\n                          bg-orange-500\n                          rounded-full\n                        \"\n                      >\n                        <a class=\"mx-2 px-2 rounded-md\">Indisponible</a>\n                      </a>\n                    </div>\n                  </td>\n                 \n                  \n<!-- settings -->\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      text-sm\n                      font-medium\n                      leading-5\n                      text-center\n                      border-b border-gray-200\n                      whitespace-nowrap\n                    \"\n                  >\n                \n                \n                     <!-- <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-cyan-400\n                        rounded-r rounded-l\n                        \"\n                      v-if=\"GetEtatAccepter() == offre.idEtat\"\n                    >\n                      Accepter\n                    </button> -->\n                    <div  v-if=\"GetEtatEnAttente() == offre.idEtat\">\n                    <button\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-cyan-400\n                        rounded-r rounded-l\n                        \"\n                        @click=\"Valider(offre.idOffre,index)\" \n                    >\n                      Accepter\n                    </button>\n                    <button\n                    style=\"margin-left:5px\"\n                      class=\"\n                        px-4\n                        py-2\n                        text-xm\n                        font-semibold\n                        text-gray-900\n                        bg-red-400\n                        rounded-r rounded-l\n                        \"\n                    @click=\"AnnulerAccepter(offre.idOffre,index)\" \n                    >\n                      Annuler\n                    </button>\n                    </div>\n                    <input\n                      type=\"checkbox\"\n                      class=\"\n                        w-5\n                        h-5\n                        text-indigo-600\n                        rounded-md\n                        focus:ring-indigo-500\n                      \"\n                      :id=\"offre.idOffre\"\n                      :value=\"offre.idOffre\"\n                      v-model=\"checkedOffreId\"\n                     v-if=\"offre.idEtat==GetEtatNonTraite()\"\n                    />\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n            <!--  -->\n\n            <div class=\" bg-white rounded-md shadow-md\"  v-if=\"checkedOffreId.length!=0 \">\n              <div class=\"grid grid-cols-1 gap-6 sm:grid-cols-2\">\n                <div class=\"text-center\">\n                  <div\n                    class=\"\n                      flex flex-col\n                      items-center\n                      bg-white\n                      xs:flex-row xs:justify-between\n                    \"\n                  >\n                    <div class=\"inline-flex xs:mt-0\"></div>\n                  </div>\n                </div>\n                <div class=\"text-center\">\n                  <div class=\"grid grid-cols-1 gap-6 sm:grid-cols-2\"  >\n                    <div\n                      class=\"\n                        py-2\n                        flex flex-col\n                        items-center\n                        bg-white\n                        xs:flex-row xs:justify-between\n                      \"\n                    >\n                      <button\n                     \n                        class=\"\n                          px-6\n                          py-2\n                          font-semibold\n                          text-gray-900\n                          bg-gray-300\n                          rounded-r rounded-l\n                          hover:bg-gray-400\n                        \"\n                        @click=\"Accepter()\"\n                      >\n                        Accepter\n                      </button>\n                    </div>\n\n                    \n                  </div>\n                </div>\n              </div>\n            </div>\n            <!-- PAGINATION -->\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- les offre selecter :{{ checkedOffreId }}\n    <br />-->\n    <!-- {{ GetEtatEnAttente()}}  -->\n  </div>\n  <!-- </div> -->\n</template>\n<script>\nimport axios from \"axios\";\nimport Url from \"../../../store/Api\";\nimport { mapGetters } from \"vuex\";\nimport BreadCrumb from \"../../../components/Intermediaire/BreadCrumb.vue\";\n// import PaginationVue from \"../../../components/Intermediaire/pagination/PaginationVue.vue\";\nexport default {\n  components: {\n    BreadCrumb,\n    // PaginationVue,\n  },\n  data() {\n    return {\n      //listeOffre: [],\n      checkedOffreId: [],\n      accepter:true\n    };\n  },\n\n  computed: {\n    ...mapGetters([\"ListeEtatOffres\", \"ListeOffres\"]),\n  },\n  mounted() {\n    this.$store.dispatch(\"Get_EtatOffre\");\n  },\n  async created() {\n    await axios\n      .get(Url + \"DemandeLivraisons/\" + this.$route.params.id, {\n        headers: {\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      })\n      .then((res) => {\n        console.log(res.data);\n        // this.checkedOffreId = res.data[0].offre\n        //   .filter((el) => el.idEtat == this.GetEtatAccepter())\n        //   .map((el) => el.idOffre);\n\n        this.$store.dispatch(\"Get_ListeOffre\", res.data[0].offre);\n      });\n  },\n  methods: {\n    GetEtatEnAttente(){\n       return this.ListeEtatOffres.filter((el) => el.etat == \"En attente d'acceptation\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatRefuser() {\n      return this.ListeEtatOffres.filter((el) => el.etat == \"Refusé\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatAccepter() {\n      return this.ListeEtatOffres.filter((el) => el.etat == \"Accepté\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n     GetEtatNonTraite() {\n      return this.ListeEtatOffres.filter((el) => el.etat == \"Non traité\").map(\n        (el) => el.idEtat\n      )[0];\n    },\n    GetEtatEnCours() {\n      return this.ListeEtatOffres.filter(\n        (el) => el.etat == \"En cours de traitement\"\n      ).map((el) => el.idEtat)[0];\n    },\n    GetEtatIndisponible() {\n      return this.ListeEtatOffres.filter(\n        (el) => el.etat == \"Indisponible\"\n      ).map((el) => el.idEtat)[0];\n    },\n    Accepter() {\n         let idt=this.ListeOffres.filter(el=>el.idOffre==this.checkedOffreId[0]).map(el=>el.idTransporteur)[0];\n\n      if(this.ListeOffres.filter(el=>el.idTransporteur==idt).length>1)\n      {\n        var AccepterOffreSeule = {\n        idEtatEnCours: this.GetEtatEnCours(),\n        OffreId: this.checkedOffreId[0],\n      };\n      this.$store.dispatch(\"Accepter_Offre_seule\", AccepterOffreSeule);\n      }\n      else{\n      var AccepterOffre = {\n        idEtatEnCours: this.GetEtatEnCours(),\n        idEtatRefuser: this.GetEtatRefuser(),\n        ListeOffresId: this.checkedOffreId,\n      };\n      this.$store.dispatch(\"Accepter_Offre\", AccepterOffre);\n      }\n       this.checkedOffreId=[];\n    },\n    AnnulerAccepter(id,index) {\n       var Annuleraccepter = {\n         idOffre:id,\n         idEtatRefuser: this.GetEtatIndisponible(),\n         index: index,\n         \n      };\n      //console.log(id)\n      console.log(Annuleraccepter)\n      this.$store.dispatch(\"Annuler_Accepter\", Annuleraccepter);\n    },\n    Valider(id,index) {\n       var AccepterValider = {\n         idOffre:id,\n         idEtatAccepter: this.GetEtatAccepter(),\n         idEtatRefuser: this.GetEtatRefuser(),\n         index: index,\n         \n      };\n      //console.log(id)\n      console.log(AccepterValider)\n      this.$store.dispatch(\"Valider_Accepter\", AccepterValider);\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}