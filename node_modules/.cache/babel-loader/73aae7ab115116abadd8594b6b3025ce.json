{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport Url from \"../../store/Api\"; //import Breadcrumb from \"../../partials/Breadcrumb.vue\";\n\nexport default {\n  components: {//Banner,\n    //Breadcrumb,\n  },\n\n  data() {\n    return {\n      personnels: [],\n      demandes: [],\n      offres: [],\n      factureTransporteurPaye: \"\",\n      factureTransporteurNonPaye: \"\",\n      factureClientPaye: \"\",\n      factureClientNonPaye: \"\",\n      data: [{\n        name: \"Demande Livraison par jour\",\n        data: {\n          \"2022-01-01\": 3,\n          \"2022-01-02\": 4,\n          \"2022-01-03\": 15,\n          \"2022-01-04\": 0,\n          \"2022-01-05\": 6,\n          \"2022-01-06\": 3\n        }\n      }]\n    };\n  },\n\n  async created() {\n    await axios.get(Url + `FactureTransporteurs?sortOrder=oui`, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      this.factureTransporteurPaye = res.data.length; //console.log(this.factureClientPaye)\n    });\n    await axios.get(Url + `FactureTransporteurs?sortOrder=non`, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      this.factureTransporteurNonPaye = res.data.length; //console.log(this.factureClientNonPaye)\n    }); //facture client\n\n    await axios.get(Url + `FactureClients`, {\n      headers: {\n        Authorization: \"Bearer \" + localStorage.getItem(\"token\")\n      }\n    }).then(res => {\n      let liste = res.data;\n      this.factureClientPaye = liste.filter(el => el.payementFile != null).length;\n      this.factureClientNonPaye = liste.filter(el => el.payementFile == null).length;\n      console.log(res.data);\n    });\n  },\n\n  methods: {\n    timeFrom(X) {\n      var dates = [];\n\n      for (let I = 0; I < Math.abs(X); I++) {\n        dates.push(new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getFullYear() + \"-\" + (new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getMonth() + 1).toString().padStart(2, \"0\") + \"-\" + new Date(new Date() - (X >= 0 ? I : I - I - I) * 24 * 60 * 60 * 1000).getDate().toString().padStart(2, \"0\"));\n      }\n\n      return dates;\n    },\n\n    factureTransporteur() {\n      let liste = [];\n      liste.push([\"Payé\", this.factureTransporteurPaye]);\n      liste.push([\"Non Payé\", this.factureTransporteurNonPaye]);\n      return liste;\n    },\n\n    factureClient() {\n      let liste = [];\n      liste.push([\"Payé\", this.factureClientPaye]);\n      liste.push([\"Non Payé\", this.factureClientNonPaye]);\n      return liste;\n    },\n\n    web() {\n      let liste = [];\n      var j1 = this.timeFrom(7)[0];\n      var j2 = this.timeFrom(7)[1];\n      var j3 = this.timeFrom(7)[2];\n      var j4 = this.timeFrom(7)[3];\n      var j5 = this.timeFrom(7)[4];\n      var j6 = this.timeFrom(7)[5];\n      var j7 = this.timeFrom(7)[6];\n      var data = {\n        [j7]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j7).length,\n        [j6]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j6).length,\n        [j5]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j5).length,\n        [j4]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j4).length,\n        [j3]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j3).length,\n        [j2]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j2).length,\n        [j1]: this.ListeDemandeLivraisonsAll.filter(el => el.datecreation.substr(0, 10) == j1).length\n      };\n      var ob = {\n        name: \"Demande Livraison par jour\",\n        data: data\n      };\n      liste.push(ob);\n      return liste;\n    }\n\n  },\n  computed: { ...mapGetters([\"ListePersonnellesAll\", \"ListeClientsAll\", \"ListeTransporteursAll\", \"ListeDemandeLivraisonsAll\" // \"ListeFactureTransporteurs\"\n    ])\n  },\n\n  mounted() {\n    this.$store.dispatch(\"Get_Personnelle_All\");\n    this.$store.dispatch(\"Get_Client_All\");\n    this.$store.dispatch(\"Get_Transporteur_All\");\n    this.$store.dispatch(\"Get_DemandeLivraison_All\"); //  this.$store.dispatch(\"Get_Facture_Transporteur\");\n  }\n\n};","map":{"version":3,"mappings":"AAqKA,SAASA,UAAT,QAA2B,MAA3B;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,iBAAhB,C,CACA;;AACA,eAAe;AACbC,YAAU,EAAE,CACV;AACA;AAFU,GADC;;AAKbC,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE,EADP;AAELC,cAAQ,EAAE,EAFL;AAGLC,YAAM,EAAE,EAHH;AAILC,6BAAuB,EAAE,EAJpB;AAKLC,gCAA0B,EAAC,EALtB;AAMLC,uBAAiB,EAAE,EANd;AAOLC,0BAAoB,EAAC,EAPhB;AAQLP,UAAI,EAAE,CACJ;AACEQ,YAAI,EAAE,4BADR;AAEER,YAAI,EAAE;AACJ,wBAAc,CADV;AAEJ,wBAAc,CAFV;AAGJ,wBAAc,EAHV;AAIJ,wBAAc,CAJV;AAKJ,wBAAc,CALV;AAMJ,wBAAc;AANV;AAFR,OADI;AARD,KAAP;AAsBD,GA5BY;;AA6Bb,QAAMS,OAAN,GAAgB;AACd,UAAMZ,KAAI,CACPa,GADG,CACCZ,GAAE,GAAK,oCADR,EAC6C;AAC/Ca,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AADsC,KAD7C,EAMHC,IANG,CAMGC,GAAD,IAAS;AACb,WAAKZ,uBAAL,GAA6BY,GAAG,CAAChB,IAAJ,CAASiB,MAAtC,CADa,CAEb;AACD,KATG,CAAN;AAUG,UAAMpB,KAAI,CACVa,GADM,CACFZ,GAAE,GAAK,oCADL,EAC0C;AAC/Ca,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AADsC,KAD1C,EAMNC,IANM,CAMAC,GAAD,IAAS;AACb,WAAKX,0BAAL,GAAgCW,GAAG,CAAChB,IAAJ,CAASiB,MAAzC,CADa,CAEb;AACD,KATM,CAAN,CAXW,CAqBb;;AACE,UAAMpB,KAAI,CACVa,GADM,CACFZ,GAAE,GAAK,gBADL,EACsB;AAC3Ba,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAYC,YAAY,CAACC,OAAb,CAAqB,OAArB;AADpB;AADkB,KADtB,EAMNC,IANM,CAMAC,GAAD,IAAS;AACb,UAAIE,KAAK,GAACF,GAAG,CAAChB,IAAd;AACD,WAAKM,iBAAL,GAAuBY,KAAK,CAACC,MAAN,CAAaC,EAAE,IAAEA,EAAE,CAACC,YAAH,IAAiB,IAAlC,EAAwCJ,MAA/D;AACC,WAAKV,oBAAL,GAA0BW,KAAK,CAACC,MAAN,CAAaC,EAAE,IAAEA,EAAE,CAACC,YAAH,IAAiB,IAAlC,EAAwCJ,MAAlE;AACAK,aAAO,CAACC,GAAR,CAAYP,GAAG,CAAChB,IAAhB;AACD,KAXM,CAAN;AAYJ,GA/DY;;AAgEbwB,SAAO,EAAE;AACPC,YAAQ,CAACC,C