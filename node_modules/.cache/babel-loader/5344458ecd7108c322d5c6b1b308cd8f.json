{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nimport BreadCrumb from \"/src/components/Transporteur/ProfilBridal.vue\";\nimport axios from \"axios\";\nimport log from '../../../middleware/log';\nexport default {\n  components: {\n    BreadCrumb\n  },\n\n  data() {\n    return {\n      Profil: [],\n      nom: \"\",\n      motdepasse: \"\",\n      prenom: \"\",\n      email: \"\",\n      password: \"\",\n      cpassword: \"\",\n      user: \"\",\n      class1: \"\",\n      class2: \"\",\n      idUser: \"\",\n      image: \"\",\n      imageFile: null\n    };\n  },\n\n  async created() {\n    await axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem('idtransporteur')).then(res => {\n      console.log(res.data);\n      this.Profil = res.data;\n      this.image = res.data.imageSrc;\n      this.user = res.data.idUserNavigation;\n      this.nom = res.data.idUserNavigation.nom;\n      this.prenom = res.data.idUserNavigation.prenom;\n      this.email = res.data.idUserNavigation.email;\n      this.idUser = res.data.idUserNavigation.idUser;\n      this.motdepasse = res.data.idUserNavigation.motdepasse;\n    });\n  },\n\n  methods: {\n    Modifier() {\n      Swal.fire({\n        title: 'Entrer Ancien mot de passe pour continuer',\n        input: 'text',\n        inputAttributes: {\n          autocapitalize: 'off'\n        },\n        showCancelButton: true,\n        confirmButtonText: 'Modifier',\n        showLoaderOnConfirm: true,\n        preConfirm: login => {\n          if (login == this.motdepasse) {\n            let user = new FormData();\n            user.append(\"idUser\", this.idUser);\n            user.append(\"Nom\", this.nom);\n            user.append(\"Prenom\", this.prenom);\n            user.append(\"Email\", this.email);\n            user.append(\"societe\", localStorage.getItem('societe'));\n            user.append(\"motdepasse\", this.password);\n            user.append(\"image\", this.Profil.idUserNavigation.image);\n            user.append(\"ImageFile\", this.imageFile);\n            user.append(\"ImageSrc\", \"\");\n            axios.put(\"http://localhost:5000/api/Users/\" + this.idUser, user, {}).then(response => {\n              console.log(res.data);\n              this.user = res.data;\n              this.nom = res.data.nom;\n              this.prenom = res.data.prenom;\n              this.email = res.data.email;\n              this.image = res.data.imageSrc;\n              this.imageFile = \"\";\n\n              if (!response.ok) {\n                throw new Error(response.statusText);\n              }\n\n              return response.json();\n            }).catch(error => {\n              Swal.showValidationMessage(`Request failed: ${error}`);\n            });\n          }\n        },\n        allowOutsideClick: () => !Swal.isLoading()\n      }).then(result => {\n        if (result.value == this.motdepasse) {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Profil Modifié\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n        } else {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"error\",\n            toast: true,\n            title: \"Ancien mot de passe incorrecte!\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n        }\n      });\n    },\n\n    // ajouter image\n    FileSelected(event) {\n      this.imageFile = event.target.files[0];\n      Swal.fire({\n        title: 'Entrer Ancien mot de passe pour continuer',\n        input: 'text',\n        inputAttributes: {\n          autocapitalize: 'off'\n        },\n        showCancelButton: true,\n        confirmButtonText: 'Modifier',\n        showLoaderOnConfirm: true,\n        preConfirm: login => {\n          if (login == this.motdepasse) {\n            let user = new FormData();\n            user.append(\"idUser\", this.idUser);\n            user.append(\"Nom\", this.nom);\n            user.append(\"Prenom\", this.prenom);\n            user.append(\"Email\", this.email);\n            user.append(\"societe\", localStorage.getItem('societe'));\n            user.append(\"motdepasse\", this.password);\n            user.append(\"image\", this.Profil.idUserNavigation.image);\n            user.append(\"ImageFile\", this.imageFile);\n            user.append(\"ImageSrc\", \"\");\n            axios.put(\"http://localhost:5000/api/Users/\" + this.idUser, user, {}).then(response => {\n              console.log(res.data);\n              this.user = res.data;\n              this.nom = res.data.nom;\n              this.prenom = res.data.prenom;\n              this.email = res.data.email;\n              this.image = res.data.imageSrc;\n              this.imageFile = \"\";\n\n              if (!response.ok) {\n                throw new Error(response.statusText);\n              }\n\n              return response.json();\n            }).catch(error => {\n              Swal.showValidationMessage(`Request failed: ${error}`);\n            });\n          }\n        },\n        allowOutsideClick: () => !Swal.isLoading()\n      }).then(result => {\n        if (result.value == this.motdepasse) {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Profil Modifié\",\n            showConfirmButton: false,\n            timer: 2000\n          }).then(() => {\n            axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem('idtransporteur')).then(res => {\n              console.log(res.data);\n              this.Profil = res.data;\n              this.image = res.data.imageSrc;\n              this.user = res.data.idUserNavigation;\n              this.nom = res.data.idUserNavigation.nom;\n              this.prenom = res.data.idUserNavigation.prenom;\n              this.email = res.data.idUserNavigation.email;\n              this.idUser = res.data.idUserNavigation.idUser;\n              this.motdepasse = res.data.idUserNavigation.motdepasse;\n            });\n          });\n        } else {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"error\",\n            toast: true,\n            title: \"Ancien mot de passe incorrecte!\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA6UA,OAAOA,UAAP,MAAuB,+CAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,yBAAhB;AACA,eAAe;AACbC,YAAU,EAAE;AACVH;AADU,GADC;;AAIbI,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,SAAG,EAAE,EAFA;AAGLC,gBAAU,EAAC,EAHN;AAILC,YAAM,EAAE,EAJH;AAKLC,WAAK,EAAE,EALF;AAMLC,cAAQ,EAAE,EANL;AAOLC,eAAS,EAAE,EAPN;AAQLC,UAAI,EAAE,EARD;AASLC,YAAM,EAAE,EATH;AAULC,YAAM,EAAE,EAVH;AAWLC,YAAM,EAAE,EAXH;AAYLC,WAAK,EAAE,EAZF;AAaLC,eAAS,EAAE;AAbN,KAAP;AAeD,GApBY;;AAsBb,QAAMC,OAAN,GAAgB;AACd,UAAMjB,KAAI,CACPkB,GADG,CAEE,6CAA4CC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAF9C,EAGHC,IAHG,CAGGC,GAAD,IAAS;AACbC,aAAO,CAACtB,GAAR,CAAYqB,GAAG,CAACnB,IAAhB;AACA,WAAKC,MAAL,GAAckB,GAAG,CAACnB,IAAlB;AACA,WAAKY,KAAL,GAAaO,GAAG,CAACnB,IAAJ,CAASqB,QAAtB;AACA,WAAKb,IAAL,GAAYW,GAAG,CAACnB,IAAJ,CAASsB,gBAArB;AACA,WAAKpB,GAAL,GAAWiB,GAAG,CAACnB,IAAJ,CAASsB,gBAAT,CAA0BpB,GAArC;AACA,WAAKE,MAAL,GAAce,GAAG,CAACnB,IAAJ,CAASsB,gBAAT,CAA0BlB,MAAxC;AACA,WAAKC,KAAL,GAAac,GAAG,CAACnB,IAAJ,CAASsB,gBAAT,CAA0BjB,KAAvC;AACA,WAAKM,MAAL,GAAcQ,GAAG,CAACnB,IAAJ,CAASsB,gBAAT,CAA0BX,MAAxC;AACA,WAAKR,UAAL,GAAgBgB,GAAG,CAACnB,IAAJ,CAASsB,gBAAT,CAA0BnB,UAA1C;AACD,KAbG,CAAN;AAcD,GArCY;;AAsCboB,SAAO,EAAE;AAENC,YAAQ,GAAG;AAChBC,UAAI,CAACC,IAAL,CAAU;AACRC,aAAK,EAAE,2CADC;AAERC,aAAK,EAAE,MAFC;AAGRC,uBAAe,EAAE;AACfC,wBAAc,EAAE;AADD,SAHT;AAMRC,wBAAgB,EAAE,IANV;AAORC,yBAAiB,EAAE,UAPX;AAQRC,2BAAmB,EAAE,IARb;AASRC,kBAAU,EAAGC,KAAD,IAAW;AACpB,cAAGA,KAAK,IAAE,KAAKhC,UAAf,EAA0B;AACvB,gBAAIK,IAAG,GAAI,IAAI4B,QAAJ,EAAX;AACF5B,gBAAI,CAAC6B,MAAL,CAA