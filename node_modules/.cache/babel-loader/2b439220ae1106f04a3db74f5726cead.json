{"ast":null,"code":"import axios from \"axios\";\nimport PaginationVue from \"/src/components/Intermediaire/pagination/PaginationVue.vue\";\nexport default {\n  components: {\n    PaginationVue\n  },\n\n  data() {\n    return {\n      camions: [],\n      ajouterCamion: \"Ajouter Camion\",\n      idtransporteur: \"\",\n      currentPage: 1,\n      perPage: 5,\n      searchby: '',\n      total: '',\n      villes: [],\n      arrive: '',\n      depart: '',\n      date: ''\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/villes\").then(resp => this.villes = resp.data); //idtransporteur de cette user\n    // liste trajets de ce transporteur \n\n    axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n      axios.get(\"http://localhost:5000/api/Trajets/\" + response.data.idTransporteur + \"/idtransporteur\").then(response => {\n        this.camions = response.data;\n        console.log(this.camions);\n      }).catch(error => console.log(error));\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    ChangePage(NumPage) {\n      this.currentPage = NumPage;\n      this.perPage = this.perPage;\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Trajets/\" + response.data.idTransporteur + \"/idtransporteur\" + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&depart=\" + this.depart + \"&arrive=\" + this.arrive + \"&date=\" + this.date).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    reload() {\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Trajets/\" + response.data.idTransporteur + \"/idtransporteur\").then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    supprimertrajet(id) {\n      axios.delete(\"http://localhost:5000/api/Trajets/\" + id).then(() => {\n        axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem('moncamion') + \"/camion\").then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA2WA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,eAAe;AACbC,YAAU,EAAE;AAEVD;AAFU,GADC;;AAKbE,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,mBAAa,EAAE,gBAFV;AAGLC,oBAAc,EAAE,EAHX;AAILC,iBAAW,EAAC,CAJP;AAKLC,aAAO,EAAC,CALH;AAMLC,cAAQ,EAAC,EANJ;AAOLC,WAAK,EAAC,EAPD;AAQLC,YAAM,EAAC,EARF;AASLC,YAAM,EAAC,EATF;AAULC,YAAM,EAAC,EAVF;AAWLC,UAAI,EAAC;AAXA,KAAP;AAcD,GApBY;;AAqBbC,SAAO,GAAG;AACLf,SAAI,CACJgB,GADA,CACI,kCADJ,EAEAC,IAFA,CAEMC,IAAD,IAAW,KAAKP,MAAL,GAAcO,IAAI,CAACf,IAFnC,EADK,CAIR;AACA;;AAEAH,SAAI,CACDgB,GADH,CAEI,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJN,EAMGH,IANH,CAMSI,QAAD,IAAc;AAClBrB,WAAI,CACFgB,GADF,CAEM,uCACEK,QAAQ,CAAClB,IAAT,CAAcmB,cADhB,GAC+B,iBAHrC,EAKGL,IALH,CAKSI,QAAD,IAAc;AAClB,aAAKjB,OAAL,GAAeiB,QAAQ,CAAClB,IAAxB;AACAoB,eAAO,CAACC,GAAR,CAAY,KAAKpB,OAAjB;AACD,OARH,EASGqB,KATH,CASUC,KAAD,IAAWH,OAAO,CAACC,GAAR,CAAYE,KAAZ,CATpB;AAUD,KAjBH,EAkBGD,KAlBH,CAkBUC,KAAD,IAAWH,OAAO,CAACC,GAAR,CAAYE,KAAZ,CAlBpB;AAmBD,GA/CY;;AAiDbC,SAAO,EAAE;AACLC,cAAU,CAACC,OAAD,EAAU;AAClB,WAAKtB,WAAL,GAAiBsB,OAAjB;AACA,WAAKrB,OAAL,GAAa,KAAKA,OAAlB;AACFR,WAAI,CACHgB,GADD,CAEE,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJJ,EAMCH,IAND,CAMOI,QAAD,IAAc;AAClBrB,aAAI,CACDgB,GADH,CAEI,uCAAuCK,QAAQ,CAAClB,IAAT,CAAcmB,cAArD,GAAqE,iBAArE,GAAuF,QAAvF,GAAgG,KAAKf,WAArG,GACA,gBADA,GACiB,K