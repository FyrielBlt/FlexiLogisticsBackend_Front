ass=\"px-5 py-3 text-xs font-semibold tracking-wider text-left text-gray-600 uppercase bg-gray-100 border-b-2 border-gray-200\"\r\n                  >\r\n                    Arrive\r\n                  </th>\r\n                  <th\r\n                    class=\"px-5 py-3 text-xs font-semibold tracking-wider text-left text-gray-600 uppercase bg-gray-100 border-b-2 border-gray-200\"\r\n                  >\r\n                    Date depart\r\n                  </th>\r\n                  <th\r\n                    class=\"px-5 py-3 text-xs font-semibold tracking-wider text-left text-gray-600 uppercase bg-gray-100 border-b-2 border-gray-200\"\r\n                  >\r\n                    Settings\r\n                  </th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(u, index) in camions\" :key=\"index\">\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    {{ index + 1 }}\r\n                  </td>\r\n\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    <div class=\"flex items-center\">\r\n                      <div class=\"ml-3\">\r\n                        <p class=\"text-gray-900 whitespace-nowrap\">\r\n                          {{ u.idCamionNavigation.codevehicule }}\r\n                        </p>\r\n                      </div>\r\n                    </div>\r\n                  </td>\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    <div class=\"flex items-center\">\r\n                      <div class=\"ml-3\">\r\n                        <div v-if=\"u.idCamionNavigation.idchauffeurNavigation != null\">\r\n                          {{ u.idCamionNavigation.idchauffeurNavigation.iduserNavigation.nom }}\r\n                        </div>\r\n                        <div v-if=\"u.idCamionNavigation.idchauffeurNavigation == null\">\r\n                          <span class=\"affect\">Affecter chauffeur</span>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </td>\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    <div class=\"flex items-center\">\r\n                      <div class=\"ml-3\">\r\n                        <p class=\"text-gray-900 whitespace-nowrap\">\r\n                          {{u.idVille1Navigation.nomVille}}\r\n                        </p>\r\n                      </div>\r\n                    </div>\r\n                  </td>\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    <div class=\"flex items-center\">\r\n                      <div class=\"ml-3\">\r\n                      \r\n                          {{ u.idVille2Navigation.nomVille}}\r\n                      </div>\r\n                    </div>\r\n                  </td>\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                    <p class=\"text-gray-900 whitespace-nowrap\">\r\n                    {{ u.date}}\r\n                    </p>\r\n                  </td>\r\n\r\n                  <td\r\n                    class=\"px-5 py-5 text-sm bg-white border-b border-gray-200\"\r\n                  >\r\n                      <span class=\"text-yellow-500 flex justify-center\">\r\n                          <button\r\n                            @click=\"supprimertrajet(u.idTrajet)\"\r\n                            class=\"mx-2 px-2 rounded-md\"\r\n                          >\r\n                            <svg\r\n          