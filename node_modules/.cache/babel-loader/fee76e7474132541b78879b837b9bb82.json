{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: [\"chauffeurid\", \"iduserchauff\", \"userss\"],\n\n  data() {\n    return {\n      idchauffeur: '',\n      codevehicule: '',\n      open: false,\n      cinchauffeur: '',\n      sucess: false,\n      error: false,\n      nextpage: 1,\n      chauffeurscamions: [],\n      i: 0\n    };\n  },\n\n  created() {\n    // inserer dans chauffeurscamions les camions sans chauffeur\n    axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem(\"iduser\")).then(response => {\n      this.response.data.forEach(element => {\n        if (element.idchauffeur == null) {\n          this.chauffeurscamions[this.i] = element;\n          this.i = this.i + 1;\n        }\n      });\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    close() {\n      this.error = false;\n      this.sucess = false;\n      this.open = false;\n      location.replace(\"chauffeurs\");\n    },\n\n    modifierChauffeur() {\n      if (this.userss.iduserNavigation.nom != '' && this.userss.iduserNavigation.prenom != '' && this.userss.iduserNavigation.email != '' && this.userss.cinchauffeur != '') {\n        axios.put(\"http://localhost:5000/api/Users/\" + this.iduserchauff, {\n          iduser: this.iduserchauff,\n          nom: this.userss.iduserNavigation.nom,\n          prenom: this.userss.iduserNavigation.prenom,\n          email: this.userss.iduserNavigation.email,\n          motdepasse: this.userss.iduserNavigation.motdepasse,\n          type: 'chauffeur',\n          image: null,\n          // à changé\n          societe: localStorage.getItem('societe')\n        }).then(response => {\n          axios.put(\"http://localhost:5000/api/Chauffeurs/\" + this.chauffeurid, {\n            idchauffeur: this.chauffeurid,\n            Cinchauffeur: this.userss.cinchauffeur,\n            Idsociete: localStorage.getItem('societe'),\n            Iduser: this.iduserchauff\n          }).then(response => {\n            axios.get(\"http://localhost:5000/api/Camions/\" + this.codevehicule + \"/camion/codevehicule\").then(response => {\n              axios.put(\"http://localhost:5000/api/Camions/\" + this.camion.idcamion, {\n                idcamion: this.response.data.idcamion,\n                trajet: this.response.data.camiontrajet,\n                destination: this.response.data.destination,\n                placementactuel: this.response.data.placementactuel,\n                idtransporteur: localStorage.getItem('iduser'),\n                idchauffeur: this.chauffeurid,\n                codevehicule: this.codevehicule,\n                statut: this.response.data.statut\n              }).catch(error => console.log(error));\n            }).catch(error => console.log(error));\n          }).catch(error => console.log(error));\n        }).catch(error => console.log(error));\n        this.sucess = true;\n        this.error = false;\n      } else {\n        this.error = true;\n        this.sucess = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqJE,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACVC,OAAK,EAAE,CAEV,aAFU,EAGV,cAHU,EAIV,QAJU,CADG;;AAObC,MAAG,GAAK;AACN,WAAO;AACPC,iBAAW,EAAC,EADL;AAEPC,kBAAY,EAAC,EAFN;AAGPC,UAAI,EAAC,KAHE;AAIPC,kBAAY,EAAC,EAJN;AAKPC,YAAM,EAAC,KALA;AAMPC,WAAK,EAAC,KANC;AAOPC,cAAQ,EAAC,CAPF;AAQPC,uBAAiB,EAAC,EARX;AASPC,OAAC,EAAC;AATK,KAAP;AAWD,GAnBY;;AAoBZC,SAAO,GAAE;AACP;AACDZ,SAAI,CACHa,GADD,CACK,uCAAqCC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAD1C,EAECC,IAFD,CAEOC,QAAD,IAAa;AACnB,WAAKA,QAAL,CAAcf,IAAd,CAAmBgB,OAAnB,CAA2BC,OAAM,IAAK;AAClC,YAAIA,OAAO,CAAChB,WAAR,IAAqB,IAAzB,EAA8B;AACzB,eAAKO,iBAAL,CAAuB,KAAKC,CAA5B,IAA+BQ,OAA/B;AACA,eAAKR,CAAL,GAAO,KAAKA,CAAL,GAAO,CAAd;AACL;AACH,OALD;AAMA,KATA,EAWCS,KAXD,CAWQZ,KAAD,IAAWa,OAAO,CAACC,GAAR,CAAYd,KAAZ,CAXlB;AAaD,GAnCY;;AAoCbe,SAAO,EAAE;AAEPC,SAAK,GAAE;AACL,WAAKhB,KAAL,GAAW,KAAX;AACA,WAAKD,MAAL,GAAY,KAAZ;AACA,WAAKF,IAAL,GAAU,KAAV;AACAoB,cAAQ,CAACC,OAAT,CAAiB,YAAjB;AAED,KARM;;AAURC,qBAAgB,GAAK;AACrB,UAAI,KAAKC,MAAL,CA