               w-full\r\n                mt-2\r\n                border-gray-200\r\n                rounded-md\r\n                focus:border-indigo-600\r\n                focus:ring\r\n                focus:ring-opacity-40\r\n                focus:ring-indigo-500\r\n              \"\r\n              style=\"text-align: center\"\r\n              id=\"Description\"\r\n              type=\"text\"\r\n              v-model=\"descriptionSociete\"\r\n            />\r\n          </div>\r\n           <div class=\"text-center\">\r\n            <label\r\n              class=\"text-gray-700\"\r\n              for=\"Image\"\r\n              style=\"text-align: center\"\r\n              >Image :</label\r\n            >\r\n            <input\r\n              class=\"\r\n                w-full\r\n                mt-2\r\n                border-gray-200\r\n                rounded-md\r\n                focus:border-indigo-600\r\n                focus:ring\r\n                focus:ring-opacity-40\r\n                focus:ring-indigo-500\r\n              \"\r\n              style=\"text-align: center\"\r\n              id=\"Image\"\r\n              type=\"file\"\r\n                 @change=\"FileSelected($event)\"\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n          <div class=\"flex justify-end mt-4\">\r\n            <button\r\n              @click=\"Close()\"\r\n              class=\"\r\n                p-3\r\n                px-6\r\n                py-3\r\n                mr-2\r\n                text-indigo-500\r\n                bg-transparent\r\n                rounded-lg\r\n                hover:bg-gray-100 hover:text-indigo-400\r\n                focus:outline-none\r\n              \"\r\n            >\r\n              Close\r\n            </button>\r\n            <button\r\n              @click=\"SaveModifier()\"\r\n              class=\"\r\n                px-4\r\n                py-2\r\n                text-gray-200\r\n                bg-gray-800\r\n      