ocus:outline-none\n              \"\n              v-model=\"VilleArrive\"\n            />\n          </div>\n          <div class=\"relative block mt-2 sm:mt-0\">\n            <input\n              placeholder=\"Ville Deppart\"\n              class=\"\n                block\n                w-full\n                py-2\n                pl-8\n                pr-6\n                text-xm text-gray-700\n                placeholder-gray-400\n                bg-white\n                border border-b border-gray-400\n                rounded-l rounded-r\n                appearance-none\n                sm:rounded-l-none\n                focus:bg-white\n                focus:placeholder-gray-600\n                focus:text-gray-700\n                focus:outline-none\n              \"\n              type=\"date\"\n              v-model=\"date\"\n            />\n          </div>\n        </div>\n\n        <div class=\"px-4 py-4 -mx-4 overflow-x-auto sm:-mx-8 sm:px-8\">\n          <div\n            class=\"inline-block min-w-full overflow-hidden rounded-lg shadow\"\n          >\n            <table class=\"min-w-full leading-normal\">\n              <thead>\n                <tr>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    ID\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Transporteur\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Ville Depart\n                  </th>\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Ville Arrive\n                  </th>\n\n                  <th\n                    class=\"\n                      px-5\n                      py-3\n                      text-sm\n                      font-medium\n                      text-gray-100\n                      uppercase\n                      bg-indigo-800\n                    \"\n                  >\n                    Date\n                  </th>\n                </tr>\n              </thead>\n              <tbody class=\"bg-white\">\n                <tr\n                  v-for=\"(trajet, index) in ListeTrajetsAll\"\n                  :key=\"trajet.idTrajet\"\n                >\n                  <th\n                    class=\"px-6 py-4 border-b border-gray-200 whitespace-nowrap\"\n                  >\n                    {{ index + 1 }}\n                  </th>\n                  <td\n                    class=\"\n                      px-6\n                      py-4\n                      border-b\n                      text-center\n                      border-gray-200\n                      whitespace-nowrap\n                      text-center\n                    \"\n                  >\n                    <div class=\"flex justify-around\">\n                      <div class=\"flex-shrink-0 w-10 h-10\">\n             