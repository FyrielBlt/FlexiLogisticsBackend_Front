button ajouter -->\r\n        <button\r\n          class=\"\r\n            px-4\r\n            py-2\r\n            text-xm\r\n            font-semibold\r\n            text-gray-900\r\n            bg-gray-300\r\n            rounded-r rounded-l\r\n            hover:bg-gray-400\r\n          \"\r\n          @click=\"Ajouter\"\r\n        >\r\n          <i class=\"bi bi-person-plus\"></i> Ajouter\r\n        </button>\r\n      </div>\r\n\r\n      <div class=\"mt-6\">\r\n        <div class=\"my-6 overflow-hidden bg-white rounded-md shadow\">\r\n          <table class=\"w-full text-center border-collapse\">\r\n            <thead class=\"border-b\">\r\n              <tr>\r\n                <th\r\n                  class=\"\r\n                    px-5\r\n                    py-3\r\n                    text-sm\r\n                    font-medium\r\n                    text-gray-100\r\n                    uppercase\r\n                    bg-indigo-800\r\n                  \"\r\n                >\r\n                  ID\r\n                </th>\r\n                <th\r\n                  class=\"\r\n                    px-5\r\n                    py-3\r\n                    text-sm\r\n                    font-medium\r\n                    text-gray-100\r\n                    uppercase\r\n                    bg-indigo-800\r\n                  \"\r\n                >\r\n                  Role\r\n                </th>\r\n                <th\r\n                  class=\"\r\n                    px-5\r\n                    py-3\r\n                    text-sm\r\n                    font-medium\r\n                    text-gray-100\r\n                    uppercase\r\n                    bg-indigo-800\r\n                  \"\r\n                >\r\n                  Nombre\r\n                </th>\r\n                <th\r\n                  class=\"\r\n                    px-5\r\n                    py-3\r\n                    text-sm\r\n                    font-medium\r\n                  