\n         if(this.codevehicule!='' && this.cinchauffeur==''){\r\n    \r\n      axios.post(\"http://localhost:5000/api/Camions\", {\r\n      idtransporteur:this.idtransporteur,\r\n      idchauffeur:null,\r\n      codevehicule:this.codevehicule\r\n      });\r\n      this.sucess=true\r\n      this.error=false\r\n      this.close();\r\n      \r\n       }\r\n     }}\r\n}\r\n</script>\r\n\r\n<style>\r\n.modal {\r\n  transition: opacity 0.25s ease;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}