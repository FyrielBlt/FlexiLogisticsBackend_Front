{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  props: [\"camiontable\"],\n\n  data() {\n    return {\n      idchauffeur: \"\",\n      codevehicule: \"\",\n      statut: \"\",\n      open: false,\n      cinchauffeur: \"\",\n      chauffeur: \"\",\n      sucess: false,\n      error: false,\n      camions: [],\n      nextpage: 1,\n      chauffeur: [],\n      chauffeurscamions: [],\n      i: 0\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/Chauffeurs/\" + localStorage.getItem(\"societe\") + \"/chauffeurs\").then(response => {\n      response.data.forEach(element => {\n        if (element.camion.length == 0) {\n          this.chauffeurscamions[this.i] = element;\n          this.i = this.i + 1;\n        }\n      });\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    close() {\n      this.error = false;\n      this.sucess = false;\n      this.open = false;\n      location.replace(\"camions\");\n    },\n\n    modifierCamion() {\n      if (this.camions.codevehicule != \"\" && this.camions.cinchauffeur != \"\") {\n        axios.get(\"http://localhost:5000/api/Chauffeurs/\" + this.cinchauffeur).then(response => {\n          this.chauffeur = response.data;\n          axios.put(\"http://localhost:5000/api/Camions/\" + this.camiontable.idcamion, {\n            idcamion: this.camiontable.idcamion,\n            idtransporteur: this.camiontable.idtransporteur,\n            idchauffeur: this.chauffeur.idchauffeur,\n            codevehicule: this.camiontable.codevehicule\n          }).then(() => {\n            this.sucess = true;\n            this.close();\n          });\n        }).catch(error => console.log(error));\n      } else {\n        this.error = true;\n        this.sucess = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqIA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACbC,OAAK,EAAE,CAAC,aAAD,CADM;;AAEbC,MAAI,GAAG;AACL,WAAO;AAELC,iBAAW,EAAE,EAFR;AAGLC,kBAAY,EAAE,EAHT;AAILC,YAAM,EAAE,EAJH;AAKLC,UA