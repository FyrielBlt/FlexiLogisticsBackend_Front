{"ast":null,"code":"import axios from \"axios\";\nimport PaginationVue from \"/src/components/Intermediaire/pagination/PaginationVue.vue\";\nimport ModalLayoutCamionUpdate from \"/src/views/Transporteur/Camions/ModalLayoutCamionUpdate.vue\";\nimport ModalLayoutTarjet from \"/src/views/Transporteur/Trajets/Tarjet.vue\";\nexport default {\n  components: {\n    ModalLayoutCamionUpdate,\n    ModalLayoutTarjet,\n    PaginationVue\n  },\n\n  data() {\n    return {\n      camions: [],\n      open: false,\n      ajouterCamion: \"Ajouter Camion\",\n      idtransporteur: \"\",\n      currentPage: 1,\n      perPage: 5,\n      type: \"\",\n      searchby: \"\",\n      total: \"\",\n      long: \"\",\n      idchauffeur: \"\",\n      codevehicule: \"\",\n      typecamion: \"\",\n      statut: \"\",\n      open: false,\n      cinchauffeur: \"\",\n      chauffeur: \"\",\n      chauffeurscamions: [],\n      i: 0,\n      typecamions: []\n    };\n  },\n\n  created() {\n    // liste camions de ce transporteur\n    axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem(\"idtransporteur\") + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage).then(response => {\n      this.camions = response.data;\n    }).catch(error => console.log(error));\n    axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem(\"idtransporteur\")).then(response => {\n      this.long = response.data.length;\n    }).catch(error => console.log(error)); // tous les typescamions\n\n    axios.get(\"http://localhost:5000/api/TypeCamions\").then(response => {\n      this.typecamions = response.data;\n    }).catch(error => console.log(error)); //les chauffeurs sans camion\n\n    axios.get(\"http://localhost:5000/api/Chauffeurs/\" + localStorage.getItem(\"societe\") + \"/chauffeurs\").then(response => {\n      response.data.forEach(element => {\n        if (element.camion.length == 0) {\n          this.chauffeurscamions[this.i] = element;\n          this.i = this.i + 1;\n        }\n      });\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    close() {\n      // liste camions de ce transporteur\n      axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem(\"idtransporteur\") + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage).then(response => {\n        this.camions = response.data;\n      }).catch(error => console.log(error));\n      axios.get(\"http://localhost:5000/api/Camions/\" + localStorage.getItem(\"idtransporteur\")).then(response => {\n        this.long = response.data.length;\n      }).catch(error => console.log(error)); // tous les typescamions\n\n      axios.get(\"http://localhost:5000/api/TypeCamions\").then(response => {\n        this.typecamions = response.data;\n      }).catch(error => console.log(error)); //les chauffeurs sans camion\n\n      axios.get(\"http://localhost:5000/api/Chauffeurs/\" + localStorage.getItem(\"societe\") + \"/chauffeurs\").then(response => {\n        response.data.forEach(element => {\n          if (element.camion.length == 0) {\n            this.chauffeurscamions[this.i] = element;\n            this.i = this.i + 1;\n          }\n        });\n      }).catch(error => console.log(error));\n    },\n\n    searchfunction(mot) {\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Camions/\" + response.data.idTransporteur + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&search=\" + mot).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    ChangePage(NumPage) {\n      this.currentPage = NumPage;\n      this.perPage = this.perPage;\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Camions/\" + response.data.idTransporteur + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&type=\" + this.type).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    setlocalstorage(id) {\n      localStorage.setItem(\"moncamion\", id);\n      location.replace(\"Trajetcamion\");\n    },\n\n    supprimercamion(id) {\n      this.$swal({\n        title: \"vous êtes sûr?\",\n        type: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#36c6d3\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Supprimer\",\n        cancelButtonText: \"Annuler\"\n      }).then(result => {\n        if (result.value) {\n          axios.delete(\"http://localhost:5000/api/Camions/\" + id).then(() => {});\n        }\n      });\n    },\n\n    ajouterCamionfunction() {\n      // tout les champs sont remplis\n      if (this.codevehicule != \"\" && this.cinchauffeur != \"\" && this.typecamion != \"\") {\n        axios.post(\"http://localhost:5000/api/Camions\", {\n          idtransporteur: localStorage.getItem(\"idtransporteur\"),\n          idchauffeur: this.cinchauffeur,\n          codevehicule: this.codevehicule,\n          idtype: this.typecamion\n        }).then(() => {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Camion Ajouté\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n          location.replace(\"camions\");\n        }).catch(error => {\n          console.log(error);\n          this.$swal({\n            position: \"top-end\",\n            icon: \"error\",\n            toast: true,\n            title: \"Codevéhicule réservé\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6YA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,OAAOC,uBAAP,MAAoC,6DAApC;AACA,OAAOC,iBAAP,MAA8B,4CAA9B;AACA,eAAe;AACbC,YAAU,EAAE;AACVF,2BADU;AAEVC,qBAFU;AAGVF;AAHU,GADC;;AAMbI,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,UAAI,EAAE,KAFD;AAGLC,mBAAa,EAAE,gBAHV;AAILC,oBAAc,EAAE,EAJX;AAKLC,iBAAW,EAAE,CALR;AAMLC,aAAO,EAAE,CANJ;AAOLC,UAAI,EAAE,EAPD;AAQLC,cAAQ,EAAE,EARL;AASLC,WAAK,EAAE,EATF;AAULC,UAAI,EAAE,EAVD;AAWLC,iBAAW,EAAE,EAXR;AAYLC,kBAAY,EAAE,EAZT;AAaLC,gBAAU,EAAE,EAbP;AAcLC,YAAM,EAAE,EAdH;AAeLZ,UAAI,EAAE,KAfD;AAgBLa,kBAAY,EAAE,EAhBT;AAiBLC,eAAS,EAAE,EAjBN;AAkBLC,uBAAiB,EAAE,EAlBd;AAmBLC,OAAC,EAAE,CAnBE;AAoBLC,iBAAW,EAAE;AApBR,KAAP;AAsBD,GA7BY;;AA8BbC,SAAO,GAAG;AACR;AACAzB,SAAI,CACD0B,GADH,CAEI,uCACEC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CADF,GAEE,QAFF,GAGE,KAAKlB,WAHP,GAIE,gBAJF,GAKE,KAAKC,OAPX,EASGkB,IATH,CASSC,QAAD,IAAc;AAClB,WAAKxB,OAAL,GAAewB,QAAQ,CAACzB,IAAxB;AACD,KAXH,EAYG0B,KAZH,CAYUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAZpB;AAaAhC,SAAI,CACD0B,GADH,CAEI,uCACEC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAHN,EAKGC,IALH,CAKSC,QAAD,IAAc;AAClB,WAAKf,IAAL,GAAYe,QAAQ,CAACzB,IAAT,CAAc8B,MAA1B;AACD,KAPH,EAQGJ,KARH,CAQUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CARpB,EAfQ,CAwBR;;AAEAhC,SAAI,CACD0B,GADH,CACO,uCADP,EAEGG,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKN,WAAL,GAAmBM,QAAQ,CAACzB,IAA5B;AACD,KAJH,EAKG0B,KALH,CAKUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CALpB,EA1BQ,CAiCR;;AACAhC,SAAI,CACD0B,GADH,CAEI,0CACEC,YAAY,CAACC,OAAb,CAAqB,SAArB,CADF,GAEE,aAJN,EAMGC,IANH,CAMSC,QAAD,IAAc;AAClBA,cAAQ,CAACzB,IAAT,CAAc+B,OAAd,CAAuBC,OAAD,IAAa;AACjC,YAAIA,OAAO,CAACC,MAAR,CAAeH,MAAf,IAAyB,CAA7B,EAAgC;AAC9B,eAAKb,iBAAL,CAAuB,KAAKC,CAA5B,IAAiCc,OAAjC;AACA,eAAKd,CAAL,GAAS,KAAKA,CAAL,GAAS,CAAlB;AACF;AACD,OALD;AAMD,KAbH,EAcGQ,KAdH,CAcUC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAdpB;AAeD,GA/EY;;AAiFbO,SAAO,EAAE;AACPC,SAAK,GAAG;AACR;AACAxC,WAAI,CACD0B,GADH,CAEI,uCACEC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CADF,GAEE,QAFF,GA