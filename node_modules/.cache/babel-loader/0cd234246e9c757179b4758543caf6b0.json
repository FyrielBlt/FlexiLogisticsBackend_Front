n\n    // ajouter image\n    FileSelected(event) {\n      this.imageFile = event.target.files[0];\n      let user = new FormData();\n      user.append(\"idUser\", this.idUser);\n      user.append(\"Nom\", this.nom);\n      user.append(\"Prenom\", this.prenom);\n      user.append(\"Email\", this.email);\n      user.append(\"motdepasse\", this.password);\n      user.append(\"image\", this.Profil.idUserNavigation.image);\n      user.append(\"ImageFile\", this.imageFile);\n      user.append(\"ImageSrc\", \"\");\n      axios.put(\"http://localhost:5000/api/Users/\" + this.idUser, user, {}).then(res => {\n        console.log(res.data);\n        this.user = res.data;\n        this.nom = res.data.nom;\n        this.prenom = res.data.prenom;\n        this.email = res.data.email;\n        this.image = res.data.imageSrc;\n        this.imageFile = \"\";\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+UA,OAAOA,UAAP,MAAuB,+CAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,OAAO,kCAAP;AACA,eAAe;AACbC,YAAU,EAAE;AACVF;AADU,GADC;;AAIbG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,SAAG,EAAE,EAFA;AAGLC,YAAM,EAAE,EAHH;AAILC,WAAK,EAAE,EAJF;AAKLC,cAAQ,EAAE,EALL;AAMLC,eAAS,EAAE,EANN;AAOLC,UAAI,EAAE,EAPD;AAQLC,YAAM,EAAE,EARH;AASLC,YAAM,EAAE,EATH;AAULC,YAAM,EAAE,EAVH;AAWLC,WAAK,EAAE,EAXF;AAYLC,eAAS,EAAE;AAZN,KAAP;AAcD,GAnBY;;AAqBb,QAAMC,OAAN,GAAgB;AACd,UAAMf,KAAI,CACPgB,GADG,CAEE,6CAA4CC,YAAY,CAACC,OAAb,CAAqB,gBAArB,CAF9C,EAGHC,IAHG,CAGGC,GAAD,IAAS;AACbC,aAAO,CAACC,GAAR,CAAYF,GAAG,CAAClB,IAAhB;AACA,WAAKC,MAAL,GAAciB,GAAG,CAAClB,IAAlB;AACA,WAAKW,KAAL,GAAaO,GAAG,CAAClB,IAAJ,CAASqB,QAAtB;AACA,WAAKd,IAAL,GAAYW,GAAG,CAAClB,IAAJ,CAASsB,gBAArB;AACA,WAAKpB,GAAL,GAAWgB,GAAG,CAAClB,IAAJ,CAASsB,gBAAT,CAA0BpB,GAArC;AACA,WAAKC,MAAL,GAAce,GAAG,CAAClB,IAAJ,CAASsB,gBAAT,CAA0BnB,MAAxC;AACA,WAAKC,KAAL,GAAac,GAAG,CAAClB,IAAJ,CAASsB,gBAAT,CAA0BlB,KAAvC;AACA,WAAKM,MAAL,GAAcQ,GAAG,CAAClB,IAAJ,CAASsB,gBAAT,CAA0BZ,MAAxC;AACD,KAZG,CAAN;AAaD,GAnCY;;AAoCba,SAAO,EAAE;AAENC,YAAQ,GAAG;AAEhBC,UAAI,CAACC,IAAL,CAAU;AA