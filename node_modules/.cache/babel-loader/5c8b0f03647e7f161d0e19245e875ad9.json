/\" + response.data.idTransporteur + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&search=\" + mot).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    ChangePage(NumPage) {\n      this.currentPage = NumPage;\n      this.perPage = this.perPage;\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Camions/\" + response.data.idTransporteur + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    setlocalstorage(id) {\n      localStorage.setItem(\"moncamion\", id);\n      location.replace('Trajetcamion');\n    },\n\n    reload() {\n      axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n        axios.get(\"http://localhost:5000/api/Camions/\" + response.data.idTransporteur + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage).then(response => {\n          this.camions = response.data;\n          console.log(this.camions);\n        }).catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    },\n\n    supprimercamion(id) {\n      axios.delete(\"http://localhost:5000/api/Camions/\" + id);\n      this.reload();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8PA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,oBAAP,MAAiC,0DAAjC;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,OAAOC,uBAAP,MAAoC,6DAApC;AACA,OAAOC,iBAAP,MAA8B,uDAA9B;AACA,eAAe;AACbC,YAAU,EAAE;AACVJ,wBADU;AAEVE,2BAFU;AAGVC,qBAHU;AAIVF;AAJU,GADC;;AAObI,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,mBAAa,