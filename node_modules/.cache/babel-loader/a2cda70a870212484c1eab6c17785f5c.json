ios.get(\"http://localhost:5000/api/DemandeDevis/\" + resp.data.idTransporteur + \"/transporteur\" + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage + \"&&depart=\" + this.depart + \"&arrive=\" + this.arrive + \"&date=\" + this.date + \"&today=\" + this.datedemande).then(response => {\n          this.demandes = response.data;\n        }).catch(error => console.log(error));\n      });\n    },\n\n    refuserdemande(demande) {\n      axios.put(\"http://localhost:5000/api/DemandeDevis/\" + demande.idDemandeDevis, {\n        idDemandeDevis: demande.idDemandeDevis,\n        date: demande.date,\n        idIntermediaire: demande.idIntermediaire,\n        idDemande: demande.idDemande,\n        idTransporteur: demande.idTransporteur,\n        idEtat: 1\n      }).then(() => {\n        axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(resp => {\n          axios.get(\"http://localhost:5000/api/DemandeDevis/\" + resp.data.idTransporteur + \"/transporteur\").then(response => {\n            this.demandes = response.data;\n          }).catch(error => console.log(error));\n        });\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8aA;AACA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,gBAAP,MAA6B,qDAA7B;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,eAAe;AACbC,YAAU,EAAE;AACVF,oBADU;AAEVC,iBAFU;AAGVE,QAAI,EAAC;AAHK,GADC;;AAMbC,MAAI,GAAG;AACL,WAAO;AACLC,cAAQ,EAAE,EADL;AAELC,iBAAW,EAAC,EAFP;AAGLC,WAAK,EAAC,IAAIC,IAAJ,GAAWC,WAAX,KAAyB,IAAzB,IAA+B,IAAID,IAAJ,GAAWE,QAAX,KAAsB,CAArD,IAAwD,GAAxD,GAA4D,IAAIF,IAAJ,GAAWG,OAAX,EAH7D;AAILC,UAAI,EAAEd,GAAG,CAAC,KAAD,CAJJ;AAKLe,iBAAW,EAAE,EALR;AAMLC,YAAM,EAAE,EANH;AAOLC,YAAM,EAAE,EAPH;AAQLC,aAAO,EAAE,KARJ;AASLC,YAAM,EAAC,EATF;AAUJC,iBAAW,EAAC,CAVR;AAWJC,cAAQ,EAAC,EAXL;AAYJC,UAAI,EAAC,EAZD;AAaLC,aAAO,EAAC,CAbH;AAcLF,cAAQ,EAAC,EAdJ;AAeLG,WAAK,EAAC;AAfD,KAAP;AAiBD,GAxBY;;AA0BbC,SAAO,GAAG;AACPxB,SAAI,CACFyB,GADF,CACM,kCADN,EAEEC,IAFF,CAEQC,IAAD,IAAW,KAAKT,M