\n      axios.put(\"http://localhost:5000/api/FactureClients/\"+u.idFactClient, facture)\r\n      .then((resp)=>{\r\n        this.open = false;\r\n    \r\n        this.$emit('f',true) \r\n          \r\n      })\r\n\r\n    },\r\n    dragFile(e) {\r\n      this.File = e.dataTransfer.files[0];\r\n      this.active = 'mt-1 flex justify-center px-6 pt-5 pb-6 border-2 border-gray-300 border-dashed rounded-md bg-gray-200'\r\n\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n</style>"]},"metadata":{},"sourceType":"module"}