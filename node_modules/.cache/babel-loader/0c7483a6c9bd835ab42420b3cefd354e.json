{"ast":null,"code":"import axios from \"axios\";\nimport datetime from \"vuejs-datetimepicker\";\nexport default {\n  props: [\"add\", \"desc\", \"prix\", \"demandeid2\", \"tabledemande2\"],\n  components: {\n    datetime\n  },\n\n  data() {\n    return {\n      idchauffeur: \"\",\n      codevehicule: localStorage.getItem(\"moncamion\"),\n      Destinaton: \"\",\n      depart: \"\",\n      statut: \"\",\n      open: false,\n      cinchauffeur: \"\",\n      chauffeur: \"\",\n      sucess: false,\n      error: false,\n      dateheure: \"\",\n      nextpage: 1,\n      chauffeurscamions: [],\n      idtransporteur: \"\",\n      i: 0,\n      camion: \"\",\n      villes: [],\n      iddepart: \"\",\n      iddestination: \"\",\n      chauffeurscamions: [],\n      encours: [],\n      accepte: []\n    };\n  },\n\n  created() {\n    //get etatdemandedevis where etat=accepte\n    axios.get(\"http://localhost:5000/api/EtatDemandeDevis/EtatDemandeDevis?etat=Accepte\").then(response => {\n      this.accepte = response.data;\n    }), //get etatoffre where etat=en cours\n    axios.get(\"http://localhost:5000/api/EtatOffres/offre?offre=Nontraite\").then(response => {\n      this.encours = response.data;\n    }), axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(resp => {\n      axios.get(\"http://localhost:5000/api/Camions/\" + resp.data.idTransporteur).then(response => {\n        this.chauffeurscamions = response.data;\n      }).catch(error => console.log(error));\n    });\n    axios.get(\"http://localhost:5000/api/Villes\").then(response => {\n      this.villes = response.data;\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    close() {\n      this.error = false;\n      this.sucess = false;\n      this.open = false;\n\n      if (this.add == false) {\n        location.replace(\"Trajetcamion\");\n      } else {\n        location.replace(\"demandechauffeur\");\n      }\n    },\n\n    ajouterTrajet() {\n      if (this.depart != \"\" && this.Destinaton != \"\" && this.depart != this.Destinaton) {\n        axios.get(\"http://localhost:5000/api/Villes/\" + this.depart + \"/ville\").then(response => {\n          this.iddepart = response.data.idVille;\n          axios.get(\"http://localhost:5000/api/Villes/\" + this.Destinaton + \"/ville\").then(response => {\n            this.iddestination = response.data.idVille;\n            axios.post(\"http://localhost:5000/api/Trajets\", {\n              idVille1: this.iddepart,\n              idVille2: this.iddestination,\n              idCamion: this.camion,\n              date: this.dateheure.toString().substr(0, 10),\n              heurdepart: this.dateheure.toString().substr(11, 16)\n            }).then(() => {\n              axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(resp => {\n                axios.post(\"http://localhost:5000/api/Offres\", {\n                  description: this.desc,\n                  date: this.dateheure.toString().substr(0, 10),\n                  idEtat: this.encours.idEtat,\n                  prix: this.prix,\n                  prixFinale: null,\n                  idTransporteur: resp.data.idTransporteur,\n                  idDemande: this.demandeid2,\n                  heurdepart: this.dateheure.toString().substr(11, 16),\n                  idCamion: this.camion\n                }).then(() => {\n                  axios.put(\"http://localhost:5000/api/DemandeDevis/\" + this.tabledemande2.idDemandeDevis, {\n                    idDemandeDevis: this.tabledemande2.idDemandeDevis,\n                    dateEnvoit: this.tabledemande2.dateEnvoit,\n                    idIntermediaire: this.tabledemande2.idIntermediaire,\n                    idDemande: this.tabledemande2.idDemande,\n                    idTransporteur: this.tabledemande2.idTransporteur,\n                    idEtat: this.accepte.idEtat\n                  });\n                }); // this.close()\n              });\n            });\n          });\n        });\n      }\n\n      if (this.depart == \"\" && this.Destinaton == \"\" && this.camion == \"\" && this.add == false) {\n        this.error = true;\n        this.sucess = false;\n      } else if (this.depart == this.Destinaton && this.add == false) {\n        this.error2 = true;\n        this.sucess = false;\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwNA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,sBAArB;AAEA,eAAe;AACbC,OAAK,EAAE,CAAC,KAAD,EAAQ,MAAR,EAAgB,MAAhB,EAAwB,YAAxB,EAAsC,eAAtC,CADM;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,EADR;AAELC,kBAAY,EAAEC,YAAY,CAACC,OAAb,CAAqB,WAArB,CAFT;AAGLC,gBAAU,EAAE,EAHP;AAILC,YAAM,EAAE,EAJH;AAKLC,YAAM,EAAE,EALH;AAMLC,UAAI,EAAE,KAND;AAOLC,kBAAY,EAAE,EAPT;AAQLC,eAAS,EAAE,EARN;AASLC,YAAM,EAAE,KATH;AAULC,WAAK,EAAE,KAVF;AAWLC,eAAS,EAAE,EAXN;AAYLC,cAAQ,EAAE,CAZL;AAaLC,uBAAiB,EAAE,EAbd;AAcLC,oBAAc,EAAE,EAdX;AAeLC,OAAC,EAAE,CAfE;AAgBLC,YAAM,EAAE,EAhBH;AAiBLC,YAAM,EAAE,EAjBH;AAkBLC,cAAQ,EAAE,EAlBL;AAmBLC,mBAAa,EAAE,EAnBV;AAoBLN,uBAAiB,EAAE,EApBd;AAqBLO,aAAO,EAAE,EArBJ;AAsBLC,aAAO,EAAE;AAtBJ,KAAP;AAwBD,GA9BY;;AA+BbC,SAAO,GAAG;AACR;AACA5B,SAAI,CACD6B,GADH,CACO,0EADP,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKJ,OAAL,GAAeI,QAAQ,CAAC3B,IAAxB;AACD,KAJH,GAKE;AACAJ,SAAI,CACD6B,GADH,CACO,4DADP,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKL,OAAL,GAAeK,QAAQ,CAAC3B,IAAxB;AACD,KAJH,CANF,EAWEJ,KAAI,CACD6B,GADH,CAEI,6CACEtB,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJN,EAMGsB,IANH,CAMSE,IAAD,IAAU;AACdhC,WAAI,CACD6B,GADH,CAEI,uCAAuCG,IAAI,CAAC5B,IAAL,CAAU6B,cAFrD,EAIGH,IAJH,CAISC,QAAD,IAAc;AAClB,aAAKZ,iBAAL,GAAyBY,QAAQ,CAAC3B,IAAlC;AACD,OANH,EAOG8B,KAPH,CAOUlB,KAAD,IAAWmB,OAAO,CAACC,GAAR,CAAYpB,KAAZ,CAPpB;AAQD,KAfH,CAXF;AA2BAhB,SAAI,CACD6B,GADH,CACO,kCADP,EAEGC,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKR,MAAL,GAAcQ,QAAQ,CAAC3B,IAAvB;AACD,KAJH,EAKG8B,KALH,CAKUlB,KAAD,IAAWmB,OAAO,CAACC,GAAR,CAAYpB,KAAZ,CALpB;AAMD,GAlEY;;AAmEbqB,SAAO,EAAE;AACPC,SAAK,GAAG;AACN,WAAKtB,KAAL,GAAa,KAAb;AACA,WAAKD,MAAL,GAAc,KAAd;AACA,WAAKH,IAAL,GAAY,KAAZ;;AACA,UAAI,KAAK2B,GAAL,IAAY,KAAhB,EAAuB;AACrBC,gBAAQ,CAACC,OAAT,CAAiB,cAAjB;AACF,OAFA,MAEO;AACLD,gBAAQ,CAACC,OAAT,CAAiB,kBAAjB;AACF;AACD,KAVM;;AAWPC,iBAAa,GAAG;AACd,UACE,KAAKhC,MAAL,IAAe,EAAf,IACA,KAAKD,UAAL,IAAmB,EADnB,IAEA,KAAKC,MAAL,IAAe,KAAKD,UAHtB,EAIE;AACAT,aAAI,CACD6B,GADH,CACO,sCAAsC,KAAKnB,MAA3C,GAAoD,QAD3D,EAEGoB,IAFH,CAESC,QAAD,IAAc;AAClB,eAAKP,QAAL,GAAgBO,QAAQ,CAAC3B,IAAT,CAAcuC,OAA9B;AACA3C,eAAI,CACD6B,GADH,CAEI,sCAAsC,KAAKpB,UAA3C,GAAwD,QAF5D,EAIGqB,IAJH,CAISC,QAAD,IAAc;AAClB,iBAAKN,aAAL,GAAqBM,QAAQ,CAAC3B,IAAT,CAAcuC,OAAnC;AACA3C,iBAAI,CACD4C,IADH,CACQ,mCADR,EAC6C;AACzCC,sBAAQ,EAAE,KAAKrB,QAD0B;AAEzCsB,sBAAQ,EAAE,KAAKrB,aAF0B;AAGzCsB,sBAAQ,EAAE,KAAKzB,MAH0B;AAIzC0B,kBAAI,EAAE,KAAK/B,SAAL,CAAegC,QAAf,GAA0BC,MAA1B,CAAiC,CAAjC,EAAoC,EAApC,CAJmC;AAKzCC,wBAAU,EAAE,KAAKlC,SAAL,CAAegC,QAAf,GAA0BC,MAA1B,CAAiC,EAAjC,EAAqC,EAArC;AAL6B,aAD7C,EAQGpB,IARH,CAQQ,MAAM;AACV9B,mBAAI,CACD6B,GADH,CAEI,6CACEtB,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,SAJN,EAMGsB,IANH,CAMSE,IAAD,IAAU;AACdhC,qBAAI,CACD4C,IADH,CACQ,kCADR,EAC4C;AACxCQ,6BAAW,EAAE,KAAKC,IADsB;AAExCL,sBAAI,EAAE,KAAK/B,SAAL,CAAegC,QAAf,GAA0BC,MAA1B,CAAiC,CAAjC,EAAoC,EAApC,CAFkC;AAGxCI,wBAAM,EAAE,KAAK5B,OAAL,CAAa4B,MAHmB;AAIxCC,sBAAI,EAAE,KAAKA,IAJ6B;AAKxCC,4BAAU,EAAE,IAL4B;AAMxCvB,gCAAc,EAAED,IAAI,CAAC5B,IAAL,CAAU6B,cANc;AAOxCwB,2BAAS,EAAE,KAAKC,UAPwB;AAQxCP,4BAAU,EAAE,KAAKlC,SAAL,CACTgC,QADS,GAETC,MAFS,CAEF,EAFE,EAEE,EAFF,CAR4B;AAWxCH,0BAAQ,EAAE,KAAKzB;AAXyB,iBAD5C,EAcGQ,IAdH,CAcQ,MAAM;AACV9B,uBAAK,CAAC2D,GAAN,CACE,4CACE,KAAKC,aAAL,CAAmBC,cAFvB,EAGE;AACEA,kCAAc,EACZ,KAAKD,aAAL,CAAmBC,cAFvB;AAGEC,8BAAU,EAAE,KAAKF,aAAL,CAAmBE,UAHjC;AAIEC,mCAAe,EACb,KAAKH,aAAL,CAAmBG,eALvB;AAMEN,6BAAS,EAAE,KAAKG,aAAL,CAAmBH,SANhC;AAOExB,kCAAc,EACZ,KAAK2B,aAAL,CAAmB3B,cARvB;AASEqB,0BAAM,EAAE,KAAK3B,OAAL,CAAa2B;AATvB,mBAHF;AAeD,iBA9BH,EADc,CAgCd;AACD,eAvCH;AAwCD,aAjDH;AAkDD,WAxDH;AAyDD,SA7DH;AA8DF;;AAEA,UACE,KAAK5C,MAAL,IAAe,EAAf,IACA,KAAKD,UAAL,IAAmB,EADnB,IAEA,KAAKa,MAAL,IAAe,EAFf,IAGA,KAAKiB,GAAL,IAAY,KAJd,EAKE;AACA,aAAKvB,KAAL,GAAa,IAAb;AACA,aAAKD,MAAL,GAAc,KAAd;AACF,OARA,MAQO,IAAI,KAAKL,MAAL,IAAe,KAAKD,UAApB,IAAkC,KAAK8B,GAAL,IAAY,KAAlD,EAAyD;AAC9D,aAAKyB,MAAL,GAAc,IAAd;AACA,aAAKjD,MAAL,GAAc,KAAd;AACF;AACD;;AA7FM;AAnEI,CAAf","names":["axios","datetime","props","components","data","idchauffeur","codevehicule","localStorage","getItem","Destinaton","depart","statut","open","cinchauffeur","chauffeur","sucess","error","dateheure","nextpage","chauffeurscamions","idtransporteur","i","camion","villes","iddepart","iddestination","encours","accepte","created","get","then","response","resp","idTransporteur","catch","console","log","methods","close","add","location","replace","ajouterTrajet","idVille","post","idVille1","idVille2","idCamion","date","toString","substr","heurdepart","description","desc","idEtat","prix","prixFinale","idDemande","demandeid2","put","tabledemande2","idDemandeDevis","dateEnvoit","idIntermediaire","error2"],"sourceRoot":"","sources":["C:\\Users\\Karim Omrane\\Desktop\\PFE FI\\FRONT\\src\\views\\Transporteur\\Trajets\\ModalTrajetAdd .vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Breadcrumb -->\r\n    <!--  <Breadcrumb breadcrumb=\"Modal\" /> -->\r\n    <button @click=\"open = true\">\r\n      <svg\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\n        width=\"30\"\r\n        height=\"25\"\r\n        fill=\"currentColor\"\r\n        class=\"bi bi-plus-circle-fill\"\r\n        viewBox=\"0 0 16 16\"\r\n      >\r\n        <path\r\n          d=\"M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z\"\r\n        />\r\n      </svg>\r\n    </button>\r\n\r\n    <div\r\n      :class=\"`modal ${\r\n        !open && 'opacity-0 pointer-events-none'\r\n      } z-50 fixed w-full h-full top-0 left-0 flex items-center justify-center`\"\r\n    >\r\n      <div\r\n        @click=\"open = false\"\r\n        class=\"absolute w-full h-full bg-gray-900 opacity-50 modal-overlay\"\r\n      ></div>\r\n\r\n      <div\r\n        class=\"\r\n          z-50\r\n          w-11/12\r\n          mx-auto\r\n          overflow-y-auto\r\n          bg-white\r\n          rounded\r\n          shadow-lg\r\n          modal-container\r\n          md:max-w-md\r\n        \"\r\n      >\r\n        <div\r\n          class=\"\r\n            absolute\r\n            top-0\r\n            right-0\r\n            z-50\r\n            flex flex-col\r\n            items-center\r\n            mt-4\r\n            mr-4\r\n            text-sm text-white\r\n            cursor-pointer\r\n            modal-close\r\n          \"\r\n        >\r\n          <svg\r\n            class=\"text-white fill-current\"\r\n            xmlns=\"http://www.w3.org/2000/svg\"\r\n            width=\"18\"\r\n            height=\"18\"\r\n            viewBox=\"0 0 18 18\"\r\n          >\r\n            <path\r\n              d=\"M14.53 4.53l-1.06-1.06L9 7.94 4.53 3.47 3.47 4.53 7.94 9l-4.47 4.47 1.06 1.06L9 10.06l4.47 4.47 1.06-1.06L10.06 9z\"\r\n            />\r\n          </svg>\r\n          <span class=\"text-sm\">(Esc)</span>\r\n        </div>\r\n\r\n        <!-- Add margin if you want to see some of the overlay behind the modal-->\r\n        <div class=\"px-6 py-4 text-left modal-content\">\r\n          <!--Title-->\r\n          <div class=\"flex items-center justify-between pb-3\">\r\n            <p class=\"text-2xl font-bold\">Ajouter Trajet</p>\r\n          </div>\r\n\r\n          <!--Body-->\r\n          <div class=\"mt-8\">\r\n            <div class=\"mt-4\">\r\n              <div class=\"p-6 bg-white rounded-md shadow-md\">\r\n                <div class=\"grid grid-cols-1 gap-6 mt-4 sm:grid-cols-2\">\r\n                  <div v-if=\"this.add != true\">\r\n                    <label class=\"text-gray-700\" for=\"passwordConfirmation\">\r\n                      camion\r\n                    </label>\r\n                    <select\r\n                      v-model=\"camion\"\r\n                      class=\"\r\n                        w-full\r\n                        mt-2\r\n                        border-gray-200\r\n                        rounded-md\r\n                        focus:border-indigo-600\r\n                        focus:ring\r\n                        focus:ring-opacity-40\r\n                        focus:ring-indigo-500\r\n                      \"\r\n                    >\r\n                      <option\r\n                        v-for=\"column in this.chauffeurscamions\"\r\n                        :key=\"column.codevehicule\"\r\n                        :value=\"column.idcamion\"\r\n                      >\r\n                        {{ column.codevehicule }}\r\n                      </option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >ville départ\r\n                  </label>\r\n                  <select\r\n                    v-model=\"depart\"\r\n                    class=\"\r\n                      w-full\r\n                      mt-2\r\n                      border-gray-200\r\n                      rounded-md\r\n                      focus:border-indigo-600\r\n                      focus:ring\r\n                      focus:ring-opacity-40\r\n                      focus:ring-indigo-500\r\n                    \"\r\n                  >\r\n                    <option v-for=\"column in this.villes\" :key=\"column\">\r\n                      {{ column.nomVille }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >Destination\r\n                  </label>\r\n                  <select\r\n                    v-model=\"Destinaton\"\r\n                    class=\"\r\n                      w-full\r\n                      mt-2\r\n                      border-gray-200\r\n                      rounded-md\r\n                      focus:border-indigo-600\r\n                      focus:ring\r\n                      focus:ring-opacity-40\r\n                      focus:ring-indigo-500\r\n                    \"\r\n                  >\r\n                    <option v-for=\"column in this.villes\" :key=\"column\">\r\n                      {{ column.nomVille }}\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div>\r\n                  <label class=\"text-gray-700\" for=\"passwordConfirmation\"\r\n                    >Date/heure\r\n                  </label>\r\n                  <br />\r\n                  <input\r\n                    type=\"datetime-local\"\r\n                    width=\"300px\"\r\n                    v-model=\"dateheure\"\r\n                  />\r\n                </div>\r\n                <div class=\"flex justify-end mt-4\">\r\n                  <button\r\n                    @click=\"ajouterTrajet\"\r\n                    class=\"\r\n                      px-4\r\n                      py-2\r\n                      text-gray-200\r\n                      bg-gray-800\r\n                      rounded-md\r\n                      hover:bg-gray-700\r\n                      focus:outline-none focus:bg-gray-700\r\n                    \"\r\n                  >\r\n                    Save\r\n                  </button>\r\n                </div>\r\n\r\n                <div v-if=\"this.error == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-red-500\"\r\n                      >Merci de remplir tout les champs</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">error!</p>\r\n                  </div>\r\n                </div>\r\n                <div v-if=\"this.error2 == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-red-500\"\r\n                      >Depart et Destination ne peuvent pas etre\r\n                      identiques</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">error!</p>\r\n                  </div>\r\n                </div>\r\n                <div v-if=\"this.sucess == true\" class=\"px-4 py-2 -mx-3\">\r\n                  <div class=\"mx-3\">\r\n                    <span class=\"font-semibold text-green-500\"\r\n                      >Nouvelle véhicule de transport</span\r\n                    >\r\n                    <p class=\"text-sm text-gray-600\">Ajout réussi!</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport datetime from \"vuejs-datetimepicker\";\r\n\r\nexport default {\r\n  props: [\"add\", \"desc\", \"prix\", \"demandeid2\", \"tabledemande2\"],\r\n  components: {\r\n    datetime,\r\n  },\r\n  data() {\r\n    return {\r\n      idchauffeur: \"\",\r\n      codevehicule: localStorage.getItem(\"moncamion\"),\r\n      Destinaton: \"\",\r\n      depart: \"\",\r\n      statut: \"\",\r\n      open: false,\r\n      cinchauffeur: \"\",\r\n      chauffeur: \"\",\r\n      sucess: false,\r\n      error: false,\r\n      dateheure: \"\",\r\n      nextpage: 1,\r\n      chauffeurscamions: [],\r\n      idtransporteur: \"\",\r\n      i: 0,\r\n      camion: \"\",\r\n      villes: [],\r\n      iddepart: \"\",\r\n      iddestination: \"\",\r\n      chauffeurscamions: [],\r\n      encours: [],\r\n      accepte: [],\r\n    };\r\n  },\r\n  created() {\r\n    //get etatdemandedevis where etat=accepte\r\n    axios\r\n      .get(\"http://localhost:5000/api/EtatDemandeDevis/EtatDemandeDevis?etat=Accepte\")\r\n      .then((response) => {\r\n        this.accepte = response.data;\r\n      }),\r\n      //get etatoffre where etat=en cours\r\n      axios\r\n        .get(\"http://localhost:5000/api/EtatOffres/offre?offre=Nontraite\")\r\n        .then((response) => {\r\n          this.encours = response.data;\r\n        }),\r\n      axios\r\n        .get(\r\n          \"http://localhost:5000/api/Transporteurs/\" +\r\n            localStorage.getItem(\"iduser\") +\r\n            \"/iduser\"\r\n        )\r\n        .then((resp) => {\r\n          axios\r\n            .get(\r\n              \"http://localhost:5000/api/Camions/\" + resp.data.idTransporteur\r\n            )\r\n            .then((response) => {\r\n              this.chauffeurscamions = response.data;\r\n            })\r\n            .catch((error) => console.log(error));\r\n        });\r\n    axios\r\n      .get(\"http://localhost:5000/api/Villes\")\r\n      .then((response) => {\r\n        this.villes = response.data;\r\n      })\r\n      .catch((error) => console.log(error));\r\n  },\r\n  methods: {\r\n    close() {\r\n      this.error = false;\r\n      this.sucess = false;\r\n      this.open = false;\r\n      if (this.add == false) {\r\n        location.replace(\"Trajetcamion\");\r\n      } else {\r\n        location.replace(\"demandechauffeur\");\r\n      }\r\n    },\r\n    ajouterTrajet() {\r\n      if (\r\n        this.depart != \"\" &&\r\n        this.Destinaton != \"\" &&\r\n        this.depart != this.Destinaton\r\n      ) {\r\n        axios\r\n          .get(\"http://localhost:5000/api/Villes/\" + this.depart + \"/ville\")\r\n          .then((response) => {\r\n            this.iddepart = response.data.idVille;\r\n            axios\r\n              .get(\r\n                \"http://localhost:5000/api/Villes/\" + this.Destinaton + \"/ville\"\r\n              )\r\n              .then((response) => {\r\n                this.iddestination = response.data.idVille;\r\n                axios\r\n                  .post(\"http://localhost:5000/api/Trajets\", {\r\n                    idVille1: this.iddepart,\r\n                    idVille2: this.iddestination,\r\n                    idCamion: this.camion,\r\n                    date: this.dateheure.toString().substr(0, 10),\r\n                    heurdepart: this.dateheure.toString().substr(11, 16),\r\n                  })\r\n                  .then(() => {\r\n                    axios\r\n                      .get(\r\n                        \"http://localhost:5000/api/Transporteurs/\" +\r\n                          localStorage.getItem(\"iduser\") +\r\n                          \"/iduser\"\r\n                      )\r\n                      .then((resp) => {\r\n                        axios\r\n                          .post(\"http://localhost:5000/api/Offres\", {\r\n                            description: this.desc,\r\n                            date: this.dateheure.toString().substr(0, 10),\r\n                            idEtat: this.encours.idEtat,\r\n                            prix: this.prix,\r\n                            prixFinale: null,\r\n                            idTransporteur: resp.data.idTransporteur,\r\n                            idDemande: this.demandeid2,\r\n                            heurdepart: this.dateheure\r\n                              .toString()\r\n                              .substr(11, 16),\r\n                            idCamion: this.camion,\r\n                          })\r\n                          .then(() => {\r\n                            axios.put(\r\n                              \"http://localhost:5000/api/DemandeDevis/\" +\r\n                                this.tabledemande2.idDemandeDevis,\r\n                              {\r\n                                idDemandeDevis:\r\n                                  this.tabledemande2.idDemandeDevis,\r\n                                dateEnvoit: this.tabledemande2.dateEnvoit,\r\n                                idIntermediaire:\r\n                                  this.tabledemande2.idIntermediaire,\r\n                                idDemande: this.tabledemande2.idDemande,\r\n                                idTransporteur:\r\n                                  this.tabledemande2.idTransporteur,\r\n                                idEtat: this.accepte.idEtat,\r\n                              }\r\n                            );\r\n                          });\r\n                        // this.close()\r\n                      });\r\n                  });\r\n              });\r\n          });\r\n      }\r\n\r\n      if (\r\n        this.depart == \"\" &&\r\n        this.Destinaton == \"\" &&\r\n        this.camion == \"\" &&\r\n        this.add == false\r\n      ) {\r\n        this.error = true;\r\n        this.sucess = false;\r\n      } else if (this.depart == this.Destinaton && this.add == false) {\r\n        this.error2 = true;\r\n        this.sucess = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}