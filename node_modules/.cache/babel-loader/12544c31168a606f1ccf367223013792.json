{"ast":null,"code":"import { defineComponent } from \"vue\"; //import Breadcrumb from \"../../partials/Breadcrumb.vue\";\n\nimport axios from \"axios\";\nimport PaginationVue from \"../../components/Intermediaire/pagination/PaginationVue.vue\"; //import $ from \"jquery\";\n\nexport default defineComponent({\n  // components: { Breadcrumb },\n  components: {\n    PaginationVue\n  },\n\n  data() {\n    return {\n      offres: [],\n      page: 1,\n      perpage: 5,\n      searchtext: \"\",\n      villes: [],\n      depart: \"\",\n      arrive: \"\"\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/villes\").then(resp => this.villes = resp.data);\n    axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage).then(resp => {\n      this.offres = resp.data;\n    });\n  },\n\n  methods: {\n    search() {\n      axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage + \"&name=\" + this.searchtext).then(resp => {\n        this.offres = resp.data;\n      });\n    },\n\n    ChangePage(NumPage) {\n      this.page = NumPage;\n\n      if (this.depart != \"\" || this.arrive != \"\") {\n        axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage + \"&depart=\" + this.depart + \"&arrive=\" + this.arrive).then(resp => {\n          this.offres = resp.data;\n        });\n      } else {\n        axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\") + \"?page=\" + this.page + \"&quantityPage=\" + this.perpage).then(resp => {\n          this.offres = resp.data;\n        });\n      }\n    },\n\n    accepter(offre) {\n      axios.put(\"http://localhost:5000/api/offres/\" + offre.idOffre, {\n        idOffre: offre.idOffre,\n        description: offre.description,\n        date: offre.date,\n        idEtat: 3,\n        prix: offre.prix,\n        idTransporteur: offre.idTransporteur,\n        idDemande: offre.idDemande,\n        recu: offre.recu,\n        file: offre.file\n      }).then(() => {\n        axios.put(\"http://localhost:5000/api/demandelivraisons/\" + offre.idDemande, {\n          idDemande: offre.idDemande,\n          description: offre.idDemandeNavigation.description,\n          datecreation: offre.idDemandeNavigation.datecreation,\n          adressdepart: offre.idDemandeNavigation.adressdepart,\n          adressarrive: offre.idDemandeNavigation.adressarrive,\n          idEtatdemande: 3,\n          idclient: offre.idDemandeNavigation.idclientNavigation.idclient,\n          file: offre.idDemandeNavigation.file\n        });\n        axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\")).then(resp => {\n          this.offres = resp.data;\n        });\n      });\n    },\n\n    annuler(offre) {\n      axios.put(\"http://localhost:5000/api/offres/\" + offre.idOffre, {\n        idOffre: offre.idOffre,\n        description: offre.description,\n        date: offre.date,\n        idEtat: 2,\n        prix: offre.prix,\n        idTransporteur: offre.idTransporteur,\n        idDemande: offre.idDemande,\n        recu: offre.recu,\n        file: offre.file\n      }).then(() => {\n        axios.put(\"http://localhost:5000/api/demandelivraisons/\" + offre.idDemande, {\n          idDemande: offre.idDemande,\n          description: offre.idDemandeNavigation.description,\n          datecreation: offre.idDemandeNavigation.datecreation,\n          adressdepart: offre.idDemandeNavigation.adressdepart,\n          adressarrive: offre.idDemandeNavigation.adressarrive,\n          idEtatdemande: 2,\n          idclient: offre.idDemandeNavigation.idclientNavigation.idclient,\n          file: offre.idDemandeNavigation.file\n        });\n        axios.get(\"http://localhost:5000/api/offres/client/\" + localStorage.getItem(\"iduser\")).then(resp => {\n          this.offres = resp.data;\n        });\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AA4ZA,SAASA,eAAT,QAAgC,KAAhC,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,6DAA1B,C,CAEA;;AACA,eAAeF,eAAe,CAAC;AAC7B;AACAG,YAAU,EAAE;AACVD;AADU,GAFiB;;AAK7BE,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,UAAI,EAAE,CAFD;AAGLC,aAAO,EAAE,CAHJ;AAILC,gBAAU,EAAE,EAJP;AAKLC,YAAM,EAAE,EALH;AAMLC,YAAM,EAAE,EANH;AAOLC,YAAM,EAAE;AAPH,KAAP;AASD,GAf4B;;AAgB7BC,SAAO,GAAG;AACRX,SAAI,CACDY,GADH,CACO,kCADP,EAEGC,IAFH,CAESC,IAAD,IAAW,KAAKN,MAAL,GAAcM,IAAI,CAACX,IAFtC;AAGAH,SAAI,CACDY,GADH,CAEI,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKX,IAHP,GAIE,gBAJF,GAKE,KAAKC,OAPX,EASGO,IATH,CASSC,IAAD,IAAU;AACd,WAAKV,MAAL,GAAcU,IAAI,CAACX,IAAnB;AACD,KAXH;AAYD,GAhC4B;;AAkC7Bc,SAAO,EAAE;AACPC,UAAM,GAAG;AACPlB,WAAI,CACDY,GADH,CAEI,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKX,IAHP,GAIE,gBAJF,GAKE,KAAKC,OALP,GAME,QANF,GAOE,KAAKC,UATX,EAWGM,IAXH,CAWSC,IAAD,IAAU;AACd,aAAKV,MAAL,GAAcU,IAAI,CAACX,IAAnB;AACD,OAbH;AAcD,KAhBM;;AAiBPgB,cAAU,CAACC,OAAD,EAAU;AAClB,WAAKf,IAAL,GAAYe,OAAZ;;AACA,UAAI,KAAKX,MAAL,IAAe,EAAf,IAAqB,KAAKC,MAAL,IAAe,EAAxC,EAA4C;AAC1CV,aAAI,CACDY,GADH,CAEI,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKX,IAHP,GAIE,gBAJF,GAKE,KAAKC,OALP,GAME,UANF,GAOE,KAAKG,MAPP,GAQE,UARF,GASE,KAAKC,MAXX,EAaGG,IAbH,CAaSC,IAAD,IAAU;AACd,eAAKV,MAAL,GAAcU,IAAI,CAACX,IAAnB;AACD,SAfH;AAgBF,OAjBA,MAiBO;AACLH,aAAI,CACDY,GADH,CAEI,6CACEG,YAAY,CAACC,OAAb,CAAqB,QAArB,CADF,GAEE,QAFF,GAGE,KAAKX,IAHP,GAIE,gBAJF,GAKE,KAAKC,OAPX,EASGO,IATH,CASSC,IAAD,IAAU;AACd,eAAKV,MAAL,GAAcU,IAAI,CAACX,IAAnB;AACD,SAXH;AAYF;AACD,KAlDM;;AAoDPkB,YAAQ,CAACC,KAAD,EAAQ;AACdtB,WAAI,CACDuB,GADH,CACO,sCAAsCD,KAAK,CAACE,OADnD,EAC4D;AACxDA,eAAO,EAAEF,KAAK,CAACE,OADyC;AAExDC,mBAAW,EAAEH,KAAK,CAACG,WAFqC;AAGxDC,YAAI,EAAEJ,KAAK,CAACI,IAH4C;AAIxDC,cAAM,EAAE,CAJgD;AAKxDC,YAAI,EAAEN,KAAK,CAACM,IAL4C;AAMxDC,sBAAc,EAAEP,KAAK,CAACO,cA