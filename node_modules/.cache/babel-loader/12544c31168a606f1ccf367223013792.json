-blue-600\r\n                focus:outline-none\r\n              \"\r\n              aria-label=\"Default select example\"\r\n            >\r\n              <option value=\"\" class=\"font-semibold\" selected>--Vide--</option>\r\n              <option v-for=\"v in villes\" :key=\"v\" :value=\"v.nomVille\">\r\n                --{{ v.nomVille }}--\r\n              </option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <table class=\"min-w-full leading-normal\" id=\"myTable\">\r\n          <thead>\r\n            <tr>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n                style=\"padding: 15px 30px\"\r\n              >\r\n                Demande\r\n              </th>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                Description\r\n              </th>\r\n              <th\r\n                class=\"\r\n                  px-5\r\n                  py-3\r\n                  text-xs\r\n                  font-semibold\r\n                  tracking-wider\r\n                  text-left text-gray-600\r\n                  uppercase\r\n                  bg-gray-100\r\n                  border-b-2 border-gray-200\r\n                \"\r\n              >\r\n                Date\r\n              </th>\r\n              <th\r\n                class=\"\r\n   