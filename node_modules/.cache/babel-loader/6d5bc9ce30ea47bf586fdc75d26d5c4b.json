{"ast":null,"code":"import axios from \"axios\";\nimport BreadCrumb from \"../../components/Intermediaire/BreadCrumb.vue\";\nexport default {\n  components: {\n    BreadCrumb\n  },\n\n  data() {\n    return {\n      offre: [],\n      demande: [],\n      etat: [],\n      transporteur: [],\n      fichieroffre: [],\n      etatdemande: [],\n      fichierdemande: []\n    };\n  },\n\n  created() {\n    axios.get(\"http://localhost:5000/api/offres/details/\" + this.$route.params.id).then(resp => {\n      this.offre = resp.data;\n      this.demande = this.offre.idDemandeNavigation;\n      this.etat = this.offre.idEtatNavigation;\n      this.transporteur = this.offre.idTransporteurNavigation.idUserNavigation;\n      this.fichieroffre = this.offre.fileOffre;\n      this.etatdemande = this.offre.idDemandeNavigation.idEtatdemandeNavigation;\n      this.fichierdemande = this.offre.idDemandeNavigation.fileDemandeLivraison;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAgIA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,UAAP,MAAuB,+CAAvB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,aAAO,EAAE,EAFJ;AAGLC,UAAI,EAAE,EAHD;AAILC,kBAAY,EAAC,EAJR;AAKLC,kBAAY,EAAC,EALR;AAMLC,iBAAW,EAAE,EANR;AAOLC,oBAAc,EAAC;AAPV,KAAP;AASD,GAdY;;AAebC,SAAO,GAAG;AACRX,SAAK,CAACY,GAAN,CAAU,8CAA8C,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EAA3E,EACGC,IADH,CACSC,IAAD,IAAU;AACd,WAAKb,KAAL,GAAaa,IAAI,CAACd,IAAlB;AACA,WAAKE,OAAL,GAAe,KAAKD,KAAL,CAAWc,mBAA1B;AACA,WAAKZ,IAAL,GAAY,KAAKF,KAAL,CAAWe,gBAAvB;AACA,WAAKZ,YAAL,GAAoB,KAAKH,KAAL,CAAWgB,wBAAX,CAAoCC,gBAAxD;AACA,WAAKb,YAAL,GAAoB,KAAKJ,KAAL,CAAWkB,SAA/B;AACA,WAAKb,WAAL,GAAmB,KAAKL,KAAL,CAAWc,mBAAX,CAA+BK,uBAAlD;AACA,WAAKb,cAAL,GAAsB,KAAKN,KAAL,CAAWc,mBAAX,CAA+BM,oBAArD;AAED,KAVH;AAWF;;AA3Ba,CAAf","names":["axios","BreadCrumb","components","data","offre","demande","etat","transporteur","fichieroffre","etatdemande","fichierdemande","created","get","$route","params","id","then","resp","idDemandeNavigation","idEtatNavigation","idTrans