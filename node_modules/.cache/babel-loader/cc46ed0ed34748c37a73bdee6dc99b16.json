ion.idDemandeDevis,\n              date: resp.data.idDemandeNavigation.date,\n              idIntermediaire: resp.data.idDemandeNavigation.idIntermediaire,\n              idDemande: resp.data.idDemandeNavigation.idDemande,\n              idTransporteur: resp.data.idDemandeNavigation.idTransporteur,\n              idEtat: this.refus\n            });\n            axios.get(\"http://localhost:5000/api/villes\").then(resp => this.villes = resp.data);\n            axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n              axios.get(\"http://localhost:5000/api/offres/\" + response.data.idTransporteur + \"/offrestransporteur\").then(resp => {\n                this.long = resp.data.length;\n              });\n            });\n            axios.get(\"http://localhost:5000/api/Transporteurs/\" + localStorage.getItem(\"iduser\") + \"/iduser\").then(response => {\n              axios.get(\"http://localhost:5000/api/offres/\" + response.data.idTransporteur + \"/offrestransporteur\" + \"?page=\" + this.currentPage + \"&quantityPage=\" + this.perPage).then(resp => {\n                this.offres = resp.data;\n              });\n            });\n          });\n          this.$swal(\"Annulation!\", \"Offre annul√©\", \"reussi\");\n        }\n      });\n    }\n\n  }\n});","map":{"version":3,"mappings":"AAslBA,SAASA,eAAT,QAAgC,KAAhC,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,4DAA1B;AACA,OAAOC,sBAAP,MAAmC,2DAAnC;AAEA,eAAeH,eAAe,CAAC;AAC7BI,YAAU,EAAE;AAAC;AACZD,0BADW;AAEXD;AAFW,GADiB;;AAK7BG,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAEPC,YAAM,EAAC,EAFA;AAGPC,WAAK,EAAC,EAHC;AAIJC,iBAAW,EAAC,CAJR;AAKJC,cAAQ,EAAC,EALL;AAMJC,UAAI,EAAC,EAND;AAOLC,aAAO,EAAC,CAPH;AAQLC,WAAK,EAAC,EARD;AASLC,UAAI,EAAC,EATA;AAULC,YAAM,EAAC,EAVF;AAWLC,YAAM,EAAC,EAXF;AAYJC,UAAI,EAAC;AAZD,KAAP;AAcD,GApB4B;;AAsB7BC,SAAO,GAAG;AACRjB,SAAI,CACDkB,GADH,CAEI,yEAFJ,EAIGC,IAJH,CAISC,QAAD,IAAa;AACjB,WAAKb,KAAL,GAAYa,QAAQ,CAAChB,IAAT