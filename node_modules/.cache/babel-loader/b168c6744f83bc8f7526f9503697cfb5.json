{"ast":null,"code":"import axios from \"axios\";\nimport url from \"../../../store/Api\";\nexport default {\n  props: [\"camiontable\"],\n\n  data() {\n    return {\n      idchauffeur: \"\",\n      codevehicule: \"\",\n      statut: \"\",\n      open: false,\n      chauffeur: \"\",\n      sucess: false,\n      error: false,\n      camions: [],\n      nextpage: 1,\n      chauffeurscamions: [],\n      typecamions: [],\n      i: 0,\n      idchauffeurcamion: \"\"\n    };\n  },\n\n  created() {\n    // tous les typescamions\n    axios.get(url + \"TypeCamions\").then(response => {\n      this.typecamions = response.data;\n    }).catch(error => console.log(error)); // les chauffeur disponible\n\n    axios.get(url + \"Chauffeurs/\" + localStorage.getItem(\"societe\") + \"/chauffeurs\").then(response => {\n      response.data.forEach(element => {\n        if (element.camion.length == 0) {\n          this.chauffeurscamions[this.i] = element;\n          this.i = this.i + 1;\n        }\n      });\n    }).catch(error => console.log(error));\n  },\n\n  methods: {\n    close() {\n      this.error = false;\n      this.sucess = false;\n      this.open = false;\n      location.replace(\"camions\");\n    },\n\n    modifierCamion() {\n      if (this.camiontable.codevehicule != \"\" && this.idchauffeurcamion != \"\" && this.camiontable.idchauffeurNavigation == null) {\n        axios.put(url + \"Camions/\" + this.camiontable.idcamion, {\n          idcamion: this.camiontable.idcamion,\n          idtransporteur: this.camiontable.idtransporteur,\n          idchauffeur: this.idchauffeurcamion,\n          codevehicule: this.camiontable.codevehicule,\n          idtype: this.camiontable.idtypeNavigation.idType\n        }).then(() => {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Camion modifié\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n          location.replace(\"camions\");\n          this.close();\n        }).catch(error => console.log(error));\n      }\n\n      if (this.camiontable.codevehicule != \"\" && this.idchauffeurcamion == \"\" && this.camiontable.idchauffeurNavigation == null) {\n        axios.put(url + \"Camions/\" + this.camiontable.idcamion, {\n          idcamion: this.camiontable.idcamion,\n          idtransporteur: this.camiontable.idtransporteur,\n          idchauffeur: null,\n          codevehicule: this.camiontable.codevehicule,\n          idtype: this.camiontable.idtypeNavigation.idType\n        }).then(() => {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Camion modifié\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n          location.replace(\"camions\");\n          this.close();\n        }).catch(error => console.log(error));\n      }\n\n      if (this.camiontable.codevehicule != \"\" && this.idchauffeurcamion == \"\" && this.camiontable.idchauffeurNavigation != null) {\n        axios.put(url + \"Camions/\" + this.camiontable.idcamion, {\n          idcamion: this.camiontable.idcamion,\n          idtransporteur: this.camiontable.idtransporteur,\n          idchauffeur: this.camiontable.idchauffeurNavigation.idchauffeur,\n          codevehicule: this.camiontable.codevehicule,\n          idtype: this.camiontable.idtypeNavigation.idType\n        }).then(() => {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"success\",\n            toast: true,\n            title: \"Camion modifié\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n          location.replace(\"camions\");\n          this.close();\n        }).catch(error => {\n          this.$swal({\n            position: \"top-end\",\n            icon: \"error\",\n            toast: true,\n            title: \"Codevéhicule réservé\",\n            showConfirmButton: false,\n            timer: 2000\n          });\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+KA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,GAAP,MAAgB,oBAAhB;AACA,eAAe;AACbC,OAAK,EAAE,CAAC,aAAD,CADM;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,iBAAW,EAAE,EADR;AAELC,kBAAY,EAAE,EAFT;AAGLC,YAAM,EAAE,EAHH;AAILC,UAAI,EAAE,KAJD;AAKLC,eAAS,EAAE,EALN;AAMLC,YAAM,EAAE,KANH;AAOLC,WAAK,EAAE,KAPF;AAQLC,aAAO,EAAE,EARJ;AASLC,cAAQ,EAAE,CATL;AAULC,uBAAiB,EAAE,EAVd;AAWLC,iBAAW,EAAE,EAXR;AAYLC,OAAC,EAAE,CAZE;AAaLC,uBAAiB,EAAE;AAbd,KAAP;AAeD,GAlBY;;AAmBbC,SAAO,GAAG;AACR;AACAjB,SAAI,CACDkB,GADH,CACOjB,GAAG,GAAC,aADX,EAEGkB,IAFH,CAESC,QAAD,IAAc;AAClB,WAAKN,WAAL,GAAmBM,QAAQ,CAACjB,IAA5B;AACD,KAJH,EAKGkB,KALH,CAKUX,KAAD,IAAWY,OAAO,CAACC,GAAR,CAAYb,KAAZ,CALpB,EAFQ,CASR;;AACAV,SAAI,CACDkB,GADH,CAEIjB,GAAG,GAAC,aAAJ,GACEuB,YAAY,CAACC,OAAb,CAAqB,SAArB,CADF,GAEE,aAJN,EAMGN,IANH,CAMSC,QAAD,IAAc;AAClBA,cAAQ,CAACjB,IAAT,CAAcuB,OAAd,CAAuBC,OAAD,IAAa;AACjC,YAAIA,OAAO,CAACC,MAAR,CAAeC,MAAf,IAAyB,CAA7B,EAAgC;AAC9B,eAAKhB,iBAAL,CAAuB,KAAKE,CAA5B,IAAiCY,OAAjC;AACA,eAAKZ,CAAL,GAAS,KAAKA,CAAL,GAAS,CAAlB;AACF;AACD,OALD;AAMD,KAbH,EAcGM,KAdH,CAcUX,KAAD,IAAWY,OAAO,CAACC,GAAR,CAAYb,KAAZ,CAdpB;AAeD,GA5CY;;AA6CboB,SAAO,EAAE;AACPC,SAAK,GAAG;AACN,WAAKrB,KAAL,GAAa,KAAb;AACA,WAAKD,MAAL,GAAc,KAAd;AACA,WAAKF,IAAL,GAAY,KAAZ;AACAyB,cAAQ,CAACC,OAAT,CAAiB,SAAjB;AACD,KANM;;AAQPC,kBAAc,GAAG;AACf,UACE,KAAKC,WAAL,CAAiB9B,YAAjB,IAAiC,EAAjC,IACA,KAAKW,iBAAL,IAA0B,EAD1B,IAEA,KAAKmB,WAAL,CAAiBC,qBAAjB,IAA0C,IAH5C,EAIE;AACApC,aAAI,CACDqC,GADH,CAEIpC,GAAG,GAAC,UAAJ,GAAiB,KAAKkC,WAAL,CAAiBG,QAFtC,EAGI;AACEA,kBAAQ,EAAE,KAAKH,WAAL,CAAiBG,QAD7B;AAEEC,wBAAc,EAAE,KAAKJ,WAAL,CAAiBI,cAFnC;AAGEnC,qBAAW,EAAE,KAAKY,iBAHpB;AAIEX,sBAAY,EAAE,KAAK8B,WAAL,CAAiB9B,YAJjC;AAKEmC,gBAAM,EAAE,KAAKL,WAAL,CAAiBM,gBAAjB,CAAkCC;AAL5C,SAHJ,EAWGvB,IAXH,CAWQ,MAAM;AACT,eAAKwB,KAAL,CAAW;AACdC,oBAAQ,EAAE,SADI;AAEdC,gBAAI,EAAE,SAFQ;AAGdC,iBAAK,EAAE,IAHO;AAIdC,iBAAK,EAAE,gBAJO;AAKdC,6BAAiB,EAAE,KALL;AAMdC,iBAAK,EAAE;AANO,WAAX;AAQJjB,kBAAQ,CAACC,OAAT,CAAiB